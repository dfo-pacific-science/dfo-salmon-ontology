@prefix :       <https://w3id.org/gcdfo/salmon/modules/upper-level-view#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix prov:   <http://www.w3.org/ns/prov#> .
@prefix sosa:   <http://www.w3.org/ns/sosa/> .
@prefix iop:    <https://w3id.org/iadopt/ont/> .
@prefix dwc:    <http://rs.tdwg.org/dwc/terms/> .
@prefix dwcdp:  <https://rs.tdwg.org/dwc/dp/terms/> .

: a owl:Ontology ;
  rdfs:label "Upper-level alignment view (SOSA/PROV/I-ADOPT/DwC)"@en ;
  rdfs:comment "Standalone visualization file derived from files 74/75/76/77. No gcdfo import; intended for WebVOWL inspection of upper-level model and cross-framework bridges."@en .

#################################################################
# Core classes (declared for graph visibility)
#################################################################

sosa:Observation a owl:Class .
sosa:Sampling a owl:Class .
sosa:Sample a owl:Class .
sosa:FeatureOfInterest a owl:Class .
sosa:ObservableProperty a owl:Class .
sosa:Procedure a owl:Class .
sosa:Result a owl:Class .
sosa:Sensor a owl:Class .
sosa:Sampler a owl:Class .

iop:Variable a owl:Class .
iop:Entity a owl:Class .
iop:Property a owl:Class .
iop:Constraint a owl:Class .
iop:StatisticalModifier a owl:Class .

dwc:Event a owl:Class .
dwc:Occurrence a owl:Class .
dwc:MaterialEntity a owl:Class .

# DwC-DP profile entities (kept as classes here for graph readability)
dwcdp:Survey a owl:Class .
dwcdp:Protocol a owl:Class .

#################################################################
# Object properties (declared for graph visibility)
#################################################################

sosa:hasFeatureOfInterest a owl:ObjectProperty .
sosa:hasResult a owl:ObjectProperty .
sosa:madeBySampler a owl:ObjectProperty .
sosa:usedProcedure a owl:ObjectProperty .
sosa:isSampleOf a owl:ObjectProperty .
sosa:isResultOf a owl:ObjectProperty .
sosa:observedProperty a owl:ObjectProperty .
sosa:madeBySensor a owl:ObjectProperty .

iop:hasProperty a owl:ObjectProperty .
iop:hasObjectOfInterest a owl:ObjectProperty .
iop:hasMatrix a owl:ObjectProperty .
iop:hasContextObject a owl:ObjectProperty .
iop:hasConstraint a owl:ObjectProperty .
iop:constrains a owl:ObjectProperty .
iop:hasStatisticalModifier a owl:ObjectProperty .

dwc:eventID a owl:ObjectProperty .
dwc:parentEventID a owl:ObjectProperty .
dwcdp:samplingEffortProtocolID a owl:ObjectProperty .

#################################################################
# Upper-level anchors (from queued crosswalk notes)
#################################################################

sosa:Observation rdfs:subClassOf prov:Activity .
sosa:Sampling rdfs:subClassOf prov:Activity .
sosa:Sample rdfs:subClassOf prov:Entity .
dwc:Occurrence rdfs:subClassOf prov:Activity .
dwc:MaterialEntity rdfs:subClassOf prov:Entity .
dwcdp:Survey rdfs:subClassOf prov:Activity .

#################################################################
# SOSA relation skeleton
#################################################################

sosa:Sampling sosa:hasFeatureOfInterest sosa:FeatureOfInterest .
sosa:Sampling sosa:hasResult sosa:Sample .
sosa:Sampling sosa:madeBySampler sosa:Sampler .
sosa:Sampling sosa:usedProcedure sosa:Procedure .

sosa:Sample sosa:isSampleOf sosa:FeatureOfInterest .
sosa:Sample sosa:isResultOf sosa:Sampling .

sosa:Observation sosa:hasFeatureOfInterest sosa:FeatureOfInterest .
sosa:Observation sosa:observedProperty sosa:ObservableProperty .
sosa:Observation sosa:hasResult sosa:Result .
sosa:Observation sosa:madeBySensor sosa:Sensor .
sosa:Observation sosa:usedProcedure sosa:Procedure .

#################################################################
# I-ADOPT decomposition skeleton
#################################################################

iop:Variable iop:hasProperty iop:Property .
iop:Variable iop:hasObjectOfInterest iop:Entity .
iop:Variable iop:hasMatrix iop:Entity .
iop:Variable iop:hasContextObject iop:Entity .
iop:Variable iop:hasConstraint iop:Constraint .
iop:Variable iop:hasStatisticalModifier iop:StatisticalModifier .
iop:Constraint iop:constrains iop:Entity .
iop:Constraint iop:constrains iop:Property .
iop:Constraint iop:constrains iop:StatisticalModifier .

#################################################################
# Bridge mappings from queued files (representation/conceptual)
#################################################################

sosa:FeatureOfInterest skos:closeMatch iop:Entity .
sosa:ObservableProperty skos:closeMatch iop:Property .
sosa:Sampling skos:closeMatch dwc:Event .
sosa:Sample skos:closeMatch dwc:MaterialEntity .
sosa:Procedure skos:closeMatch dwcdp:Protocol .

# DwC-DP relationship hints from queued notes
# (kept as lightweight links for graph orientation)
dwcdp:Survey dwc:eventID dwc:Event .
dwc:Occurrence dwc:eventID dwc:Event .
dwc:Event dwc:parentEventID dwc:Event .
dwcdp:Survey dwcdp:samplingEffortProtocolID dwcdp:Protocol .
