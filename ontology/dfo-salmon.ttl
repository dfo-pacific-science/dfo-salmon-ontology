#################################################################

envo:01000617 a owl:Class ;
  skos:prefLabel "lentic water body"@en ;
  iao:0000115 "A lentic water body is a water body in which the accumulated water, in its totality, has very little to no directed flow."@en ;
  rdfs:subClassOf bfo:0000040 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000020 a owl:Class ;
  skos:prefLabel "lake"@en ;
  iao:0000115 "A body of water or other liquid of considerable size contained in a depression on a landmass."@en ;
  rdfs:subClassOf envo:01000617 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000033 a owl:Class ;
  skos:prefLabel "pond"@en ;
  iao:0000115 "A body of water, usually of smaller size than a lake."@en ;
  rdfs:subClassOf envo:01000617 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:01000618 a owl:Class ;
  skos:prefLabel "lotic water body"@en ;
  iao:0000115 "A lotic water body is a water body in which the accumulated water, in its totality, is flowing."@en ;
  rdfs:subClassOf bfo:0000040 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000022 a owl:Class ;
  skos:prefLabel "river"@en ;
  iao:0000115 "A stream which, through permanent or seasonal flow processes, moves from elevated land towards lower elevations through a definite channel and empties either into a sea, lake, or another river or ends on land as bed seepage and evapotranspiration exceed water supply."@en ;
  rdfs:subClassOf envo:01000618 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000023 a owl:Class ;
  skos:prefLabel "stream"@en ;
  skos:altLabel "creek"@en ;
  rdfs:comment "A creek is typically smaller than a stream."@en ;
  iao:0000115 "A watercourse which is linear and flows across the solid portion of a planetary surface."@en ;
  rdfs:subClassOf envo:01000618 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000234 a owl:Class ;
  skos:prefLabel "bayou"@en ;
  skos:altLabel "slough"@en ;
  iao:0000115 "A small, slow-moving stream or creek; usually located in low-lying areas."@en ;
  rdfs:subClassOf envo:00000023 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

#################################################################
# Evidence Completeness Framework (DQV-based)
#################################################################

dfoc:EvidenceCompletenessDimension a dqv:Dimension ;
  skos:prefLabel "Evidence Completeness Dimension"@en ;
  skos:definition "A dimension for assessing the completeness of evidence supporting salmon stock assessments and management decisions."@en ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:DataCurrencyDimension a dqv:Dimension ;
  skos:prefLabel "Data Currency Dimension"@en ;
  skos:definition "A dimension for assessing the currency (timeliness) of data used in salmon stock assessments and management decisions."@en ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:CompleteEvidence a dqv:QualityAnnotation ;
  skos:prefLabel "Complete Evidence"@en ;
  dqv:inDimension dfoc:EvidenceCompletenessDimension ;
  skos:definition "Indicates that the evidence supporting a salmon stock assessment or management decision is complete and comprehensive."@en ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:GapsEvidence a dqv:QualityAnnotation ;
  skos:prefLabel "Gaps in Evidence"@en ;
  dqv:inDimension dfoc:EvidenceCompletenessDimension ;
  skos:definition "Indicates that there are gaps or missing information in the evidence supporting a salmon stock assessment or management decision."@en ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:MissingCriticalEvidence a dqv:QualityAnnotation ;
  skos:prefLabel "Missing Critical Evidence"@en ;
  dqv:inDimension dfoc:EvidenceCompletenessDimension ;
  skos:definition "Indicates that critical evidence is missing, significantly impacting the reliability of a salmon stock assessment or management decision."@en ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> . 


