  dfoc:theme dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:ChangeLogEntry a owl:Class ;
  rdfs:label "Advice change log entry"@en ;
  iao:0000115 "Documents a change from the previous assessment cycle, such as data updates, methodological adjustments, or altered advice."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity 
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:MetricBenchmark a owl:Class ;
  rdfs:label "Metric benchmark"@en ;
  iao:0000115 "Benchmark value for a WSP metric that helps delineate status zones (e.g., the limit reference point for absolute abundance)."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity 
  dfoc:theme dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:AlgorithmThreshold a owl:Class ;
  rdfs:label "Algorithm threshold"@en ;
  iao:0000115 "Threshold value used by the rapid-status algorithm (e.g., Learning Tree 3) that may differ from expert-defined benchmarks to incorporate precautionary buffers."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity 
  dfoc:theme dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

#################################################################
# FSAR controlled vocabularies
#################################################################

dfoc:WSPMetric a skos:Concept ;
  skos:prefLabel "WSP metric"@en ;
  skos:definition "One of the standard Wild Salmon Policy rapid-status metrics (absolute abundance, relative abundance, long-term trend, or percent change)."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity 
  skos:inScheme :WSPMetricScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:AbsoluteAbundanceMetric a dfoc:WSPMetric , skos:Concept ;
  skos:prefLabel "Absolute abundance metric"@en ;
  skos:definition "Wild Salmon Policy metric comparing generational average spawner abundance to COSEWIC criterion D1 thresholds."@en ;
  skos:inScheme :WSPMetricScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:RelativeAbundanceMetric a dfoc:WSPMetric , skos:Concept ;
  skos:prefLabel "Relative abundance metric"@en ;
  skos:definition "Wild Salmon Policy metric comparing generational average spawner abundance to CU-specific benchmarks (for example Sgen or 80% SMSY)."@en ;
  skos:inScheme :WSPMetricScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:LongTermTrendMetric a dfoc:WSPMetric , skos:Concept ;
  skos:prefLabel "Long-term trend metric"@en ;
  skos:definition "Wild Salmon Policy metric comparing current generational average to long-term average spawner abundance."@en ;
  skos:inScheme :WSPMetricScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:PercentChangeMetric a dfoc:WSPMetric , skos:Concept ;
  skos:prefLabel "Percent change metric"@en ;
  skos:definition "Wild Salmon Policy metric quantifying linear change in spawner abundance over the most recent three generations."@en ;
  skos:inScheme :WSPMetricScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:ConfidenceCategory a skos:Concept ;
  skos:prefLabel "Confidence category"@en ;
  skos:definition "Categorical confidence rating (High, Medium, Low) reported alongside a rapid status assessment."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity 
  skos:inScheme :RapidStatusConfidenceScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:RapidStatusConfidenceHigh a dfoc:ConfidenceCategory , skos:Concept ;
  skos:prefLabel "High rapid-status confidence"@en ;
  skos:definition "Rapid status confidence category indicating that high-quality abundance metrics support the result."@en ;
  skos:inScheme :RapidStatusConfidenceScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:RapidStatusConfidenceMedium a dfoc:ConfidenceCategory , skos:Concept ;
  skos:prefLabel "Medium rapid-status confidence"@en ;
  skos:definition "Rapid status confidence category indicating mixed evidence or reliance on trend metrics supplemented by some abundance information."@en ;
  skos:inScheme :RapidStatusConfidenceScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:RapidStatusConfidenceLow a dfoc:ConfidenceCategory , skos:Concept ;
  skos:prefLabel "Low rapid-status confidence"@en ;
  skos:definition "Rapid status confidence category indicating limited supporting data (e.g., derived from trend metrics alone)."@en ;
  skos:inScheme :RapidStatusConfidenceScheme ;
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

