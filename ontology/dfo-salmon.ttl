# Generic membership (transitive)
dfoc:hasMember a owl:ObjectProperty , owl:TransitiveProperty ;
  skos:prefLabel "has member"@en ;
  iao:0000115 "Generic hierarchical containment/membership across management hierarchies (transitive)."@en ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:isMemberOf a owl:ObjectProperty ;
  skos:prefLabel "is member of"@en ;
  iao:0000115 "Inverse of hasMember."@en ;
  owl:inverseOf dfoc:hasMember ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

# Stock Management Unit ▶ Conservation Unit
dfoc:hasMemberCU a owl:ObjectProperty ;
  skos:prefLabel "has member CU"@en ;
  iao:0000115 "A Stock Management Unit contains a Conservation Unit."@en ;
  rdfs:domain dfoc:StockManagementUnit ;
  rdfs:range  dfoc:ConservationUnit ;
  rdfs:subPropertyOf dfoc:hasMember ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:isMemberOfSMU a owl:ObjectProperty ;
  skos:prefLabel "is member of SMU"@en ;
  iao:0000115 "A Conservation Unit belongs to a Stock Management Unit."@en ;
  rdfs:domain dfoc:ConservationUnit ;
  rdfs:range  dfoc:StockManagementUnit ;
  owl:inverseOf dfoc:hasMemberCU ;
  rdfs:subPropertyOf dfoc:isMemberOf ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

# Conservation Unit ▶ Stock (Population)
dfoc:hasMemberStock a owl:ObjectProperty ;
  skos:prefLabel "has member stock"@en ;
  iao:0000115 "A Conservation Unit contains one or more Stocks (populations)."@en ;
  rdfs:domain dfoc:ConservationUnit ;
  rdfs:range  dfoc:Stock ;
  rdfs:subPropertyOf dfoc:hasMember ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:isMemberOfCU a owl:ObjectProperty ;
  skos:prefLabel "is member of CU"@en ;
  iao:0000115 "A Stock (population) belongs to a Conservation Unit."@en ;
  rdfs:domain dfoc:Stock ;
  rdfs:range  dfoc:ConservationUnit ;
  owl:inverseOf dfoc:hasMemberStock ;
  rdfs:subPropertyOf dfoc:isMemberOf ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

# Inference note:
# Because hasMemberCU ⊑ hasMember and hasMemberStock ⊑ hasMember, and hasMember is transitive:
# MU hasMemberCU CU  AND  CU hasMemberStock Stock  ⇒  MU hasMember Stock

