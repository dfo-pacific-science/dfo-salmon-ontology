    rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
    skos:altLabel "AUC"@en ;
    skos:broader :EnumerationMethod,
        :EstimateMethod ;
    skos:definition "Integrates serial counts over time using a survey‑life parameter; may apply observer efficiency."@en ;
    skos:inScheme :EnumerationMethodScheme,
        :EstimateMethodScheme ;
    skos:prefLabel "Area Under the Curve"@en ;
    :theme :MonitoringFieldWorkTheme,
        :StockAssessmentTheme .

:BiologicalBenchmarkAndIndicatorScheme a skos:ConceptScheme ;
    rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
    skos:definition "Controlled vocabulary for key biological benchmarks, performance indicators, and reference points used in salmon stock assessment and management (e.g., SMSY, Sgen, spawner abundance)."@en ;
    skos:hasTopConcept :TargetOrLimitRateOrAbundance ;
    skos:prefLabel "Biological Benchmark and Indicator Scheme"@en ;
    :theme :StockAssessmentTheme .

:BroodYear a owl:Class ;
