  rdfs:subClassOf iao:0000030 ; # Information content entity
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:DecisionRequirement a owl:Class ;
  rdfs:label "Decision requirement"@en ;
  iao:0000115 "A reusable pattern describing management or policy obligations that are triggered by specific assessment conditions."@en ;
  rdfs:subClassOf iao:0000104 ; # plan specification
  dfoc:theme dfoc:PolicyGovernanceTheme , dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:PolicyReadiness a owl:Class ;
  rdfs:label "Policy and legal readiness"@en ;
  iao:0000115 "Summarizes whether an assessment satisfies mandatory policy gates such as WSP rapid status disclosure, harvest control rule identification, and Fish Stocks provisions compliance."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity
  dfoc:theme dfoc:PolicyGovernanceTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:ManagementDecision a owl:Class ;
  rdfs:label "Management decision"@en ;
  iao:0000115 "A fisheries management action informed by scientific advice, such as establishing a total allowable catch or implementing time-area closures."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity 
  dfoc:theme dfoc:PolicyGovernanceTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:CUInclusion a owl:Class ;
  rdfs:label "CU accounting entry"@en ;
  iao:0000115 "Records how a specific conservation unit is treated within an assessment cycle—included, excluded, or proxied—with supporting rationale and review metadata."@en ;
  rdfs:subClassOf iao:0000030 ; # Information content entity 
  dfoc:theme dfoc:StockAssessmentTheme ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:GSIUsage a owl:Class ;
