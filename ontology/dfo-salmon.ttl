odo:SALMON_00000167 a owl:Class ;
  skos:prefLabel "Fish measurement type"@en ;
  iao:0000115 "A measurement type taken of some physical, biological, or ecological aspect of a fish or fishes."@en ;
  rdfs:subClassOf iao:0000030 ;
  rdfs:isDefinedBy <http://purl.dataone.org/odo/> .

odo:SALMON_00000479 a owl:Class ;
  skos:prefLabel "Salmon escapement count"@en ;
  iao:0000115 "The number of salmon which do not get caught by commercial or recreational fisheries and return to their freshwater spawning habitat."@en ;
  rdfs:subClassOf iao:0000109 ;
  rdfs:isDefinedBy <http://purl.dataone.org/odo/> .

odo:SALMON_00000639 a owl:Class ;
