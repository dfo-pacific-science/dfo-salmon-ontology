#################################################################
# Escapement Method hierarchy (OWL-only, replaces SKOS)
#################################################################

dfoc:EscapementMethod a owl:Class ;
  skos:prefLabel "Escapement Method"@en ;
  rdfs:subClassOf dwc:Protocol ;
  iao:0000115 "A methodological family used to estimate escapement."@en ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

# NOTE: Hyponyms of EscapementMethod. Optionally mark as disjoint (see below).
dfoc:AreaUnderTheCurve a owl:Class ;
  skos:prefLabel "Area Under the Curve"@en ;
  iao:0000115 "Estimation by integrating counts over time to approximate total escapement."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:AutomatedCountingMethods a owl:Class ;
  skos:prefLabel "Automated Counting Methods"@en ;
  iao:0000115 "Technology-based direct counts at fixed sites (e.g., resistivity, video, sonar)."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:CalibratedTimeSeries a owl:Class ;
  skos:prefLabel "Calibrated Time Series"@en ;
  iao:0000115 "Estimation using calibration of time series counts against independent measures."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:ExpansionMethods a owl:Class ;
  skos:prefLabel "Expansion Methods (multiply by factors)"@en ;
  iao:0000115 "Extrapolate peak/sampled observations using expansion factors (e.g., redd multipliers)."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:ExpertOpinion a owl:Class ;
  skos:prefLabel "Expert Opinion"@en ;
  iao:0000115 "Estimate derived via expert judgment considering multiple evidence sources."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:FixedStationCountAnalysis a owl:Class ;
  skos:prefLabel "Fixed Station Count Analysis"@en ;
  iao:0000115 "Combine/aggregate manual counts from fixed monitoring locations."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:MarkRecaptureAnalysis a owl:Class ;
  skos:prefLabel "Mark-Recapture Analysis"@en ;
  iao:0000115 "Statistical estimation from mark–recapture designs (e.g., Petersen, Jolly-Seber)."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:MathematicalOperations a owl:Class ;
  skos:prefLabel "Mathematical Operations"@en ;
  iao:0000115 "Algebraic aggregation/adjustment of observations (add/subtract/multiply/divide)."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:PeakCountAnalysis a owl:Class ;
  skos:prefLabel "Peak Count Analysis"@en ;
  iao:0000115 "Use the peak live (± dead) count as the basis for annual escapement."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

dfoc:UnknownMethod a owl:Class ;
  skos:prefLabel "Unknown Method"@en ;
  iao:0000115 "Method not documented or cannot be categorized."@en ;
  rdfs:subClassOf dfoc:EscapementMethod ;
  rdfs:isDefinedBy <https://w3id.org/dfoc/salmon> .

