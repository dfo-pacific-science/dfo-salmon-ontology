  rdfs:domain dfo:DesignatableUnit ;
  rdfs:range dfo:COSEWICStatusCategory ;
  rdfs:isDefinedBy <https://w3id.org/dfo/salmon> .

dfo:hasStockStatusZone a owl:ObjectProperty ;
  rdfs:label "has stock status zone"@en ;
  rdfs:comment "Relates a stock management unit to its current stock status zone (e.g., Healthy, Cautious, Critical) under the Precautionary Approach."@en ;
  rdfs:domain dfo:StockManagementUnit ;
