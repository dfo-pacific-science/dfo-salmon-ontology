dfo:ExpertOpinion a owl:Class ;
  rdfs:label "Expert Opinion"@en ;
  rdfs:comment "Estimate derived via expert judgment considering multiple evidence sources."@en ;
  rdfs:subClassOf dfo:EscapementMethod ;
