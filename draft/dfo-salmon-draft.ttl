#################################################################
# SKOS Enumeration Methods (from escapement_estimate_type_guidance.txt)
#################################################################

:EnumerationMethod a skos:Concept ;
  skos:prefLabel "Enumeration Method"@en ;
  skos:definition "A method used to enumerate or count salmon in the field"@en ;
  skos:inScheme :EnumerationMethodScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:CensusByManualCountAtFixedSite a skos:Concept ;
  skos:prefLabel "Census by Manual Count at Fixed Site"@en ;
  skos:definition "Salmon are guided through a controlled opening in a fence/weir where trained observers visually tally each passage event on a continuous or daily schedule. Counting chambers/windows and SIL/SEN daily logs document coverage; bypass routes are monitored and closed where possible."@en ;
  skos:inScheme :EnumerationMethodScheme ;
  skos:broader :EnumerationMethod ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:VisualSnorkelCount a skos:Concept ;
  skos:prefLabel "Visual Snorkel Count"@en ;
  skos:definition "Two or more trained snorkelers conduct standardized passes within defined segments, tallying live/dead by species. Field forms capture visibility class, segment boundaries, and repeat‑visit schedule; counts are aggregated across observers/passes."@en ;
  skos:inScheme :EnumerationMethodScheme ;
  skos:broader :EnumerationMethod ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:AerialSurveyCount a skos:Concept ;
  skos:prefLabel "Aerial Survey Count"@en ;
  skos:definition "Repeated overflights enumerate visible spawners over defined waterbodies using helicopter fixed wing or drone platforms. Flight lines altitude sensor settings and ground sampling distance are documented and imagery may be reviewed post flight."@en ;
  skos:inScheme :EnumerationMethodScheme ;
  skos:broader :EnumerationMethod ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# SKOS Estimate Methods (from escapement_estimate_type_guidance.txt)
#################################################################

:FixedStationTally a skos:Concept ;
  skos:prefLabel "Fixed‑Station Tally"@en ;
  skos:definition "Seasonal total = roll‑up of daily/continuous counts from a fixed (non‑sonar) station; simple arithmetic with documented coverage."@en ;
  skos:inScheme :EstimateMethodScheme ;
  skos:broader :EstimateMethod ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:AreaUnderTheCurve a skos:Concept ;
  skos:prefLabel "Area Under the Curve"@en ;
  skos:altLabel "AUC"@en ;
  skos:definition "Integrates serial counts over time using a survey‑life parameter; may apply observer efficiency."@en ;
  skos:inScheme :EstimateMethodScheme , :EnumerationMethodScheme;
  skos:broader :EstimateMethod , :EnumerationMethod ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:PeakCountAnalysis a skos:Concept ;
  skos:prefLabel "Peak Count Analysis"@en ;
  skos:definition "Uses the peak survey (live ± dead variants) as the season estimate/index."@en ;
  skos:inScheme :EstimateMethodScheme ;
  skos:broader :EstimateMethod ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:EstimateMethod a skos:Concept ;
  skos:prefLabel "Estimate Method"@en ;
  skos:definition "A method used to analyze and estimate salmon abundance from enumeration data"@en ;
  skos:inScheme :EstimateMethodScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# SKOS Downgrade Criteria (from escapement_estimate_type_guidance.txt)
#################################################################

:DowngradeCriteria a skos:Concept ;
  skos:prefLabel "Downgrade Criteria"@en ;
  skos:definition "Criteria used to downgrade the estimate type assigned to a salmon escapement estimate."@en ;
  skos:inScheme :DowngradeCriteriaScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:Visits a skos:Concept ;
  skos:prefLabel "Number of visits"@en ;
  skos:definition "Too few site visits for the claimed precision tier (e.g., <5 for high‑effort visual programs)."@en ;
  skos:inScheme :DowngradeCriteriaScheme ;
  skos:broader :DowngradeCriteria ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:ReachCoverage a skos:Concept ;
  skos:prefLabel "Spatial coverage"@en ;
  skos:definition "Fraction of intended reaches/segments not surveyed."@en ;
  skos:inScheme :DowngradeCriteriaScheme ;
  skos:broader :DowngradeCriteria ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:Visibility a skos:Concept ;
  skos:prefLabel "Visibility/turbidity/lighting"@en ;
  skos:definition "Poor water clarity, glare, or lighting limits detection (visual/video/sonar)."@en ;
  skos:inScheme :DowngradeCriteriaScheme ;
  skos:broader :DowngradeCriteria ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:DocEvidence a skos:Concept ;
  skos:prefLabel "Documentation/Evidence"@en ;
  skos:definition "Missing/insufficient SIL/SEN/field notes/parameter records to support the claimed Type."@en ;
  skos:inScheme :DowngradeCriteriaScheme ;
  skos:broader :DowngradeCriteria ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# SKOS Estimate Types (Hyatt 1997)
#################################################################

:Type1 a skos:Concept ;
  skos:prefLabel "Type-1, True Abundance, high resolution"@en ;
  skos:definition "Total, seasonal counts through fence or fishway; virtually no bypass. Simple, often single step analysis. Reliable resolution of between year differences >10% (in absolute units)."@en ;
  skos:inScheme :EstimateTypeScheme ;
  skos:broader :EstimateType ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:Type2 a skos:Concept ;
  skos:prefLabel "Type-2, True Abundance, medium resolution"@en ;
  skos:definition "High effort (5 or more trips), standard methods (e.g. mark-recapture, serial counts for area under curve, etc.). Simple to complex multi-step, but always rigorous. Reliable resolution of between year differences >25% (in absolute units)."@en ;
  skos:inScheme :EstimateTypeScheme ;
  skos:broader :EstimateType ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:Type3 a skos:Concept ;
  skos:prefLabel "Type-3, Relative Abundance, high resolution"@en ;
  skos:definition "High effort (5 or more trips), standard methods (e.g. equal effort surveys executed by walk, swim, overflight, etc.). Simple to complex multi-step, but always rigorous. Reliable resolution of between year differences >25% (in relative units)."@en ;
  skos:inScheme :EstimateTypeScheme ;
  skos:broader :EstimateType ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:Type4 a skos:Concept ;
  skos:prefLabel "Type-4, Relative Abundance, medium resolution"@en ;
  skos:definition "Low to moderate effort (1-4 trips), known survey method. Simple analysis by known methods. Reliable resolution of between year differences >200% (in relative units)."@en ;
  skos:inScheme :EstimateTypeScheme ;
  skos:broader :EstimateType ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:Type5 a skos:Concept ;
  skos:prefLabel "Type-5, Relative Abundance, low resolution"@en ;
  skos:definition "Low effort (e.g. 1 trip), use of vaguely defined, inconsistent or poorly executed methods. Unknown to ill defined; inconsistent or poorly executed. Uncertain numeric comparisons, but high reliability for presence or absence."@en ;
  skos:inScheme :EstimateTypeScheme ;
  skos:broader :EstimateType ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:Type6 a skos:Concept ;
  skos:prefLabel "Type-6, Presence or Absence"@en ;
  skos:definition "Any of above methods. Not required for analysis. Moderate to high reliability for presence/absence."@en ;
  skos:inScheme :EstimateTypeScheme ;
  skos:broader :EstimateType ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

:EstimateType a skos:Concept ;
  skos:prefLabel "Estimate Type"@en ;
  skos:definition "Classification of escapement estimate quality based on Hyatt 1997 framework"@en ;
  skos:inScheme :EstimateTypeScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# MIREOT Import
# Upper ontology grounding for process/entity/quality hierarchy
#################################################################
# Basic Formal Ontology (BFO)
#################################################################

bfo:0000015 a owl:Class ; # process
  rdfs:label "process"@en ;
  iao:0000115 "An occurrent that has temporal parts and for some time t, p s-depends_on some material entity at t."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/bfo.owl> .

bfo:0000020 a owl:Class ; # specifically dependent continuant
  rdfs:label "specifically dependent continuant"@en ;
  iao:0000115 "b is a specifically dependent continuant = Def. b is a continuant & there is some independent continuant c which is not a spatial region and which is such that b s-depends_on c at every time t during the course of b’s existence. (axiom label in BFO2 Reference: [050-003])"@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/bfo.owl> .

bfo:0000040 a owl:Class ; # material entity
  rdfs:label "material entity"@en ;
  iao:0000115 "An independent continuant that is spatially extended whose identity is independent of that of other entities and can be maintained through time."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/bfo.owl> .

bfo:0000031 a owl:Class ; # generically dependent continuant
  rdfs:label "generically dependent continuant"@en ;
  iao:0000115 "A continuant that is dependent on one or more independent continuants and whose instances can be multiply instantiated."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/bfo.owl> .

#################################################################
# Information Artifact Ontology (IAO)
# IAO extends BFO
#################################################################

iao:0000030 a owl:Class ; # information content entity
  rdfs:label "information content entity"@en ;
  rdfs:subClassOf bfo:0000031 ; # generically dependent continuant
  iao:0000115 "A generically dependent continuant that is about some thing."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

iao:0000033 a owl:Class ; # directive information entity
  rdfs:label "directive information entity"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "An information content entity whose concretizations indicate to their bearer how to realize them in a process."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

iao:0000104 a owl:Class ; # plan specification
  rdfs:label "plan specification"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "A directive information entity with action specifications and objective specifications as parts, and that may be concretized as a realizable entity that, if realized, is realized in a process in which the bearer tries to achieve the objectives by taking the actions specified."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

iao:0000109 a owl:Class ; # measurement datum
  rdfs:label "measurement datum"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "A measurement datum is an information content entity that is a recording of the output of a measurement such as produced by a device."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

iao:0000310 a owl:Class ; # document
  rdfs:label "document"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "A collection of information content entities intended to be understood together as a whole."@en ; # definition
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

iao:0000115 a owl:AnnotationProperty ; # definition
  rdfs:label "definition"@en ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

iao:0000119 a owl:AnnotationProperty ; # definition source
  rdfs:label "definition source"@en ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/iao.owl> .

#################################################################
# Web Annotation Ontology (OA)
#################################################################

oa:Annotation a owl:Class ;
  rdfs:label "Annotation"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "The class for Web Annotations."@en ; # definition
  iao:0000119 <http://www.w3.org/ns/oa#Annotation> ; # definition source
  rdfs:isDefinedBy <http://www.w3.org/ns/oa#> .

#################################################################
# Data Quality Vocabulary (DQV)
#################################################################

dqv:Dimension a owl:Class ;
  rdfs:label "Dimension"@en ;
  rdfs:subClassOf skos:Concept ; # this is taken directly from DQV
  iao:0000115 "Represents criteria relevant for assessing quality. Each quality dimension must have one or more metric to measure it. A dimension is linked with a category using the dqv:inCategory property."@en ; # definition
  iao:0000119 <https://www.w3.org/TR/vocab-dqv/> ; # definition source
  rdfs:isDefinedBy <http://www.w3.org/ns/dqv#> .

dqv:QualityAnnotation a owl:Class ;
  rdfs:label "Quality Annotation"@en ;
  rdfs:subClassOf oa:Annotation;
  iao:0000115 "Represents quality annotations, including ratings, quality certificates or feedback that can be associated to datasets or distributions. Quality annotations must have one oa:motivatedBy statement with an instance of oa:Motivation (and skos:Concept) that reflects a quality assessment purpose. We define this instance as dqv:qualityAssessment."@en ; # definition
  iao:0000119 <https://www.w3.org/TR/vocab-dqv/> ; # definition source
  rdfs:isDefinedBy <http://www.w3.org/ns/dqv#> .

dqv:inDimension a owl:ObjectProperty ;
  rdfs:label "in dimension"@en ;
  iao:0000115 "Represents the dimensions a quality metric, certificate and annotation allow a measurement of."@en ; # definition
  iao:0000119 <https://www.w3.org/TR/vocab-dqv/> ; # definition source
  rdfs:comment "Dimensions are meant to systematically organize metrics, quality certificates and quality annotations. The Data Quality Vocabulary defines no specific cardinality constraints for dqv:inDimension, since distinct quality frameworks might have different perspectives over a metric. A metric may therefore be associated to more than one dimension. However, those who define new quality metrics should try to avoid this as much as possible and assign only one dimension to the metrics they define. More than one dimension can be indicated for each quality annotation or certificate."@en ;
  rdfs:isDefinedBy <http://www.w3.org/ns/dqv#> .

#################################################################
# Darwin Core (DWC)
# Maps DwC classes to BFO upper ontology for logical grounding
#################################################################

# Material entities
dwc:Organism a owl:Class ;
  rdfs:label "Organism"@en ;
  rdfs:subClassOf bfo:0000040 ; # material entity
  iao:0000115 "A particular organism or defined group of organisms considered to be taxonomically homogeneous."@en ; # definition
  iao:0000119 <https://dwc.tdwg.org/list/> ; # definition source
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .

dwc:MaterialEntity a owl:Class ;
  rdfs:label "Material Entity"@en ;
  rdfs:subClassOf bfo:0000040 ; # material entity
  iao:0000115 "An entity that can be identified, exists for some period of time, and consists in whole or in part of physical matter while it exists."@en ; # definition
  iao:0000119 <https://dwc.tdwg.org/list/> ; # definition source
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .

dwc:Agent a owl:Class ;
  rdfs:label "Agent"@en ;
  rdfs:subClassOf bfo:0000040 ; # material entity
  iao:0000115 "A resource that acts or has the power to act."@en ; # definition
  iao:0000119 <https://gbif.github.io/dwc-dp/qrg/> ; # definition source
  dcterms:isVersionOf <http://purl.org/dc/terms/Agent> ;
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .    # not listed in the DwC class list but defined in DwC-DP

dwc:Media a owl:Class ;
  rdfs:label "Media"@en ;
  rdfs:subClassOf bfo:0000040 ; # material entity
  iao:0000115 "A dcmi:MediaType or other media type with other entities as subject matter."@en ; # definition
  iao:0000119 <https://gbif.github.io/dwc-dp/qrg/> ; # definition source
  dcterms:isVersionOf <http://rs.tdwg.org/ac/terms/Media> ;
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .   # not listed in the DwC class list but defined in DwC-DP

# Processes
dwc:Event a owl:Class ;
  rdfs:label "Event"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "An action that occurs at some location during some time."@en ; # definition
  iao:0000119 <https://dwc.tdwg.org/list/> ; # definition source
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .

dwc:Occurrence a owl:Class ;
  rdfs:label "Occurrence"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "An existence of a dwc:Organism at a particular place at a particular time."@en ; # definition
  iao:0000119 <https://dwc.tdwg.org/list/> ; # definition source
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .

dwc:Identification a owl:Class ;
  rdfs:label "Identification"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "A taxonomic determination (e.g., the assignment to a dwc:Taxon)."@en ; # definition
  iao:0000119 <https://dwc.tdwg.org/list/> ; # definition source
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .

# Information entities
dwc:Protocol a owl:Class ;
  rdfs:label "Protocol"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "A method used during an action."@en ; # definition
  iao:0000119 <https://gbif.github.io/dwc-dp/qrg/> ; # definition source
  rdfs:isDefinedBy <http://rs.tdwg.org/dwc/terms/> .  # not listed in the DwC class list but defined in DwC-DP

#################################################################
# Additional measurement-related classes from DataONE (ODO)
# The Ecosystem Ontology (ECSO) and SALMON
#################################################################

odo:ECSO_00002050 a owl:Class ;
  rdfs:label "year of measurement"@en ;
  iao:0000115 "year of measurement"@en ; # definition
  rdfs:subClassOf iao:0000109 ; # measurement datum
  rdfs:isDefinedBy <http://purl.dataone.org/odo/> .

odo:SALMON_00000137 a owl:Class ;
  rdfs:label "Fishery type"@en ;
  iao:0000115 "The Food and Agriculture Organization of the United Nations (FAO) defines a fishery as '...an activity leading to harvesting of fish. It may involve capture of wild fish or raising of fish through aquaculture.'"@en ; # definition
  rdfs:subClassOf bfo:0000040 ; # material entity
  rdfs:isDefinedBy <http://purl.dataone.org/odo/> .

odo:SALMON_00000167 a owl:Class ;
  rdfs:label "Fish measurement type"@en ;
  iao:0000115 "A measurement type taken of some physical, biological, or ecological aspect of a fish or fishes."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # information content entity
  rdfs:isDefinedBy <http://purl.dataone.org/odo/> .

odo:SALMON_00000479 a owl:Class ;
  rdfs:label "Salmon escapement count"@en ;
  iao:0000115 "The number of salmon which do not get caught by commercial or recreational fisheries and return to their freshwater spawning habitat."@en ; # definition
  rdfs:subClassOf iao:0000109 ; # measurement datum
  rdfs:isDefinedBy <http://purl.dataone.org/odo/> .

odo:SALMON_00000639 a owl:Class ;
  rdfs:label "Fish stock type"@en ;
  iao:0000115 "Group of individuals of a species which can be regarded as an entity for management or assessment purposes; a separate breeding population of a species; term used to identify a management unit of fishery species. A distinct genetic population, a population defined by movement pattern, part of a population potentially harvestable, or a quantity of fish from a given area. May be a total or a spawning stock."@en ; # definition
  rdfs:subClassOf bfo:0000040 ; # material entity
  rdfs:isDefinedBy <http://purl.dataone.org/odo/> .

#################################################################
# Environment Ontology (ENVO)
#################################################################

envo:01000617 a owl:Class ;
  rdfs:label "lentic water body"@en ;
  iao:0000115 "A lentic water body is a water body in which the accumulated water, in its totality, has very little to no directed flow."@en ; # definition
  rdfs:subClassOf bfo:0000040 ; # material entity
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000020 a owl:Class ;
  rdfs:label "lake"@en ;
  iao:0000115 "A body of water or other liquid of considerable size contained in a depression on a landmass."@en ; # definition
  rdfs:subClassOf envo:01000617 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000033 a owl:Class ;
  rdfs:label "pond"@en ;
  iao:0000115 "A body of water, usually of smaller size than a lake."@en ; # definition
  rdfs:subClassOf envo:01000617 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:01000618 a owl:Class ;
  rdfs:label "lotic water body"@en ;
  iao:0000115 "A lotic water body is a water body in which the accumulated water, in its totality, is flowing."@en ; # definition
  rdfs:subClassOf bfo:0000040 ; # material entity
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000022 a owl:Class ;
  rdfs:label "river"@en ;
  iao:0000115 "A stream which, through permanent or seasonal flow processes, moves from elevated land towards lower elevations through a definite channel and empties either into a sea, lake, or another river or ends on land as bed seepage and evapotranspiration exceed water supply."@en ; # definition
  rdfs:subClassOf envo:01000618 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000023 a owl:Class ;
  rdfs:label "stream"@en ;
  skos:altLabel "creek"@en ;
  rdfs:comment "A creek is typically smaller than a stream."@en ;
  iao:0000115 "A watercourse which is linear and flows across the solid portion of a planetary surface."@en ; # definition
  rdfs:subClassOf envo:01000618 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

envo:00000234 a owl:Class ;
  rdfs:label "bayou"@en ;
  skos:prefLabel "slough"@en ;
  skos:altLabel "slough"@en ;
  iao:0000115 "A small, slow-moving stream or creek; usually located in low-lying areas."@en ; # definition
  rdfs:subClassOf envo:00000023 ;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/envo.owl> .

#################################################################
# Core Organization Ontology (ORG)
# Imported to model DFO organizational structure and ensure assert FAIR
# practices by documenting project metadata
#################################################################

org:Organization a owl:Class ;
  rdfs:label "Organization"@en ;
  iao:0000115 "Represents a collection of people organized together into a community or other social, commercial or political structure. The group has some common purpose or reason for existence which goes beyond the set of people belonging to it and can act as an Agent. Organizations are often decomposable into hierarchical structures."@en ; # definition
  rdfs:isDefinedBy <http://www.w3.org/ns/org> .

org:OrganizationalUnit a owl:Class ;
  rdfs:label "Organizational Unit"@en ;
  rdfs:subClassOf org:Organization ;
  iao:0000115 "An Organization such as a department or support unit which is part of some larger Organization and only has full recognition within the context of that Organization. In particular the unit would not be regarded as a legal entity in its own right."@en ; # definition
  rdfs:isDefinedBy <http://www.w3.org/ns/org> .

org:hasUnit a owl:ObjectProperty ;
  rdfs:label "has unit"@en ;
  iao:0000115 "Indicates a unit which is part of this Organization, e.g., a Department within a larger Organization."@en ; # definition
  rdfs:domain org:Organization ;
  rdfs:range org:OrganizationalUnit ;
  rdfs:subPropertyOf org:hasSubOrganization ;
  rdfs:isDefinedBy <http://www.w3.org/ns/org> .

org:hasSubOrganization a owl:ObjectProperty ;
  rdfs:label "has sub-organization"@en ;
  iao:0000115 "Represents hierarchical containment of Organizations or OrganizationalUnits; indicates an organization which is a sub-part or child of this organization."@en ; # definition
  rdfs:domain org:Organization ;
  rdfs:range org:Organization ;
  rdfs:isDefinedBy <http://www.w3.org/ns/org> .

#################################################################
# DFO Organizational Structure
#################################################################

gcdfos:Unit a owl:Class ;
  rdfs:label "Unit"@en ;
  skos:altLabel "Program"@en ;
  rdfs:subClassOf org:OrganizationalUnit ;
  iao:0000115 "A basic operational grouping within a Section, responsible for a defined set of tasks or functions and reporting to the Section leadership."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Section a owl:Class ;
  rdfs:label "Section"@en ;
  rdfs:subClassOf org:OrganizationalUnit ;
  iao:0000115 "A mid-level organisational unit within a Branch, tasked with coordinating and delivering a coherent set of related functions or programmes under the Branch’s mandate."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Division a owl:Class ;
  rdfs:label "Division"@en ;
  rdfs:subClassOf org:OrganizationalUnit ;
  iao:0000115 "A higher-level grouping above Section, organising several Sections under a common strategic or administrative theme; responsible for overall oversight of its component Sections."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Branch a owl:Class ;
  rdfs:label "Branch"@en ;
  rdfs:subClassOf org:OrganizationalUnit ;
  iao:0000115 "A major organisational division within DFO, typically responsible for a broad functional area (such as Science, Regulation, Aquatic Ecosystems) and composed of one or more Sections."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Region a owl:Class ;
  rdfs:label "Region"@en ;
  rdfs:subClassOf org:OrganizationalUnit ;
  iao:0000115 "A geographic organisational unit covering a specified area (province(s)/territories/coastal zone) within Canada; responsible for delivering DFO programmes and services regionally, and reporting to national headquarters and the Deputy Minister structure."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Sector a owl:Class ;
  rdfs:label "Sector"@en ;
  rdfs:subClassOf org:OrganizationalUnit ;
  iao:0000115 "A top-tier organisational domain within DFO’s national structure (such as Innovation and Business Transformation, Fisheries and Harbour Management) led by an Assistant Deputy Minister; responsible for strategic leadership, policy, and national programme direction."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:hasUnit a owl:ObjectProperty ;
  rdfs:label "has unit"@en ;
  iao:0000115 "Links an organization to one of its operational units."@en ; # definition
  rdfs:subPropertyOf org:hasUnit ;
  rdfs:domain org:Organization ;
  rdfs:range gcdfos:Unit ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:hasSection a owl:ObjectProperty ;
  rdfs:label "has section"@en ;
  iao:0000115 "Links an organization to a subordinate section within its structure."@en ; # definition
  rdfs:subPropertyOf org:hasSubOrganization ;
  rdfs:domain org:Organization ;
  rdfs:range gcdfos:Section ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:hasDivision a owl:ObjectProperty ;
  rdfs:label "has division"@en ;
  iao:0000115 "Links an organization to a subordinate division that groups multiple sections."@en ; # definition
  rdfs:subPropertyOf org:hasSubOrganization ;
  rdfs:domain org:Organization ;
  rdfs:range gcdfos:Division ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:hasBranch a owl:ObjectProperty ;
  rdfs:label "has branch"@en ;
  iao:0000115 "Links an organization to a subordinate branch responsible for a major functional area."@en ; # definition
  rdfs:subPropertyOf org:hasSubOrganization ;
  rdfs:domain org:Organization ;
  rdfs:range gcdfos:Branch ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:hasRegion a owl:ObjectProperty ;
  rdfs:label "has region"@en ;
  iao:0000115 "Links an organization to a subordinate geographic region."@en ; # definition
  rdfs:subPropertyOf org:hasSubOrganization ;
  rdfs:domain org:Organization ;
  rdfs:range gcdfos:Region ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:hasSector a owl:ObjectProperty ;
  rdfs:label "has sector"@en ;
  iao:0000115 "Links an organization to one of its top-level sectors."@en ; # definition
  rdfs:subPropertyOf org:hasSubOrganization ;
  rdfs:domain org:Organization ;
  rdfs:range gcdfos:Sector ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:organizationalUnitName a owl:DatatypeProperty ;
  rdfs:label "organizational unit name"@en ;
  iao:0000115 "The human-readable name of an organizational unit within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain org:OrganizationalUnit ;
  rdfs:range xsd:string ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:organizationalUnitCode a owl:DatatypeProperty ;
  rdfs:label "organizational unit code"@en ;
  iao:0000115 "The short code or identifier assigned to an organizational unit within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain org:OrganizationalUnit ;
  rdfs:range xsd:string ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Project a owl:Class ;
  rdfs:label "Project"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "A planned undertaking with defined objectives, scope, and resources, typically involving a series of coordinated activities to achieve specific goals within a set timeframe."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:managedBy a owl:ObjectProperty ;
  rdfs:label "managed by"@en ;
  iao:0000115 "Indicates the organizational unit responsible for managing a project within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain gcdfos:Project ;
  rdfs:range gcdfos:Unit ;
  owl:inverseOf gcdfos:manages ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:collaboratesWith a owl:ObjectProperty ;
  rdfs:label "collaborates with"@en ;
  iao:0000115 "Indicates a Unit that collaborates on a project within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain gcdfos:Project ;
  rdfs:range gcdfos:Unit ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:manages a owl:ObjectProperty ;
  rdfs:label "manages"@en ;
  iao:0000115 "Indicates the project managed by a Unit within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain gcdfos:Unit ;
  rdfs:range gcdfos:Project ;
  owl:inverseOf gcdfos:managedBy ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:projectTitle a owl:DatatypeProperty ;
  rdfs:label "project title"@en ;
  iao:0000115 "The title of a project within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain gcdfos:Project ;
  rdfs:range xsd:string ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:projectFirstYear a owl:DatatypeProperty ;
  rdfs:label "project first year"@en ;
  iao:0000115 "The first year of a project within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain gcdfos:Project ;
  rdfs:range xsd:gYear ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:projectLastYear a owl:DatatypeProperty ;
  rdfs:label "project last year"@en ;
  iao:0000115 "The last year of a project within Fisheries and Oceans Canada (DFO)."@en ; # definition
  rdfs:domain gcdfos:Project ;
  rdfs:range xsd:gYear ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# Evidence Completeness Framework (DQV-based)
#################################################################
# Note that dqv:Dimension is a subclass of skos:Concept in DQV

gcdfos:EvidenceCompletenessDimension a dqv:Dimension ;
  skos:prefLabel "Evidence Completeness Dimension"@en ;
  skos:definition "A dimension for assessing the completeness of evidence supporting salmon stock assessments and management decisions."@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:DataCurrencyDimension a dqv:Dimension ;
  skos:prefLabel "Data Currency Dimension"@en ;
  skos:definition "A dimension for assessing the currency (timeliness) of data used in salmon stock assessments and management decisions."@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CompleteEvidence a dqv:QualityAnnotation ;
  skos:prefLabel "Complete Evidence"@en ;
  dqv:inDimension gcdfos:EvidenceCompletenessDimension ;
  skos:definition "Indicates that the evidence supporting a salmon stock assessment or management decision is complete and comprehensive."@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:GapsEvidence a dqv:QualityAnnotation ;
  skos:prefLabel "Gaps in Evidence"@en ;
  dqv:inDimension gcdfos:EvidenceCompletenessDimension ;
  skos:definition "Indicates that there are gaps or missing information in the evidence supporting a salmon stock assessment or management decision."@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:MissingCriticalEvidence a dqv:QualityAnnotation ;
  skos:prefLabel "Missing Critical Evidence"@en ;
  dqv:inDimension gcdfos:EvidenceCompletenessDimension ;
  skos:definition "Indicates that critical evidence is missing, significantly impacting the reliability of a salmon stock assessment or management decision."@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# Base superclasses
#################################################################

gcdfos:ObservedRateOrAbundance a owl:Class ;
  rdfs:label "Observed rate or abundance"@en ;
  iao:0000115 "A measurement datum representing an empirically observed rate, count, or abundance derived from monitoring or survey data."@en ; # definition
  rdfs:subClassOf iao:0000109 ; # measurement datum
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:TargetOrLimitRateOrAbundance a owl:Class ;
  rdfs:label "Target or limit rate or abundance"@en ;
  iao:0000115 "A value specification representing a policy- or model-defined target or limit, such as TAC, UMSY, or reference points."@en ; # definition
  rdfs:subClassOf iao:0000109 ; # measurement datum
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ReportingOrManagementStratum a owl:Class ;
  rdfs:label "Reporting or management stratum"@en ;
  iao:0000115 "An information-defined grouping of fish populations used for reporting, conservation assessment, or management purposes. Examples include Conservation Units (CUs), Stock Management Units (MUs), and Designatable Units (DUs)."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# Core classes
#################################################################

gcdfos:AgeAtMaturity a owl:Class ;
  rdfs:label "Age-at-maturity"@en ;
  iao:0000115 "The age when salmon become sexually mature adults. Influenced by genetics and environment."@en ; # definition
  iao:0000119 "Van Beveren, E. (2024). Revision of Catch- and Maturity- at Age Used to Assess the Northern Contingent of Atlantic Mackerel (Scomber scombrus). DFO Can. Sci. Advis. Sec. Res. Doc. 2024/012. iv + 28 p."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  rdfs:subClassOf gcdfos:ObservedRateOrAbundance .

gcdfos:YearBasis a owl:Class ;
  rdfs:label "Year basis"@en ;
  iao:0000115 "A year-based temporal reference used to contextualize measurements or rates (e.g., brood year or catch year)."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:BroodYear a owl:Class ;
  rdfs:label "Brood year"@en ;
  rdfs:subClassOf gcdfos:YearBasis ;
  iao:0000115 "The year that a cohort of salmon spawns."@en ; # definition
  iao:0000119 "Government of Canada Pacific Salmon Glossary; verbatim in NCEAS Salmon Ontology."@en ; # definition source
  dcterms:source <https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html> , <http://purl.dataone.org/odo/SALMON_00000520> ;
  skos:exactMatch odo:SALMON_00000520 ; # "Brood Year" from NCEAS Salmon Ontology (ODO)
  rdfs:comment "The parental year for a group of returning salmon, i.e. the calendar year when the majority of parents of these fish spawned."@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CatchYear a owl:Class ;
  rdfs:label "Catch year"@en ;
  rdfs:subClassOf gcdfos:YearBasis ;
  iao:0000115 "The calendar year in which returning salmon are caught in fisheries (as opposed to the brood year in which the cohort was spawned)."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:hasYearBasis a owl:ObjectProperty ;
  rdfs:label "has year basis"@en ;
  iao:0000115 "Links a measurement or rate to the year basis (e.g., brood year or catch year) it is reported against."@en ; # definition
  rdfs:domain [ owl:unionOf ( gcdfos:ExploitationRate gcdfos:TotalExploitationRate gcdfos:Catch gcdfos:Recruitment gcdfos:SpawnerAbundance gcdfos:EscapementMeasurement ) ] ;
  rdfs:range [ owl:unionOf ( gcdfos:BroodYear gcdfos:CatchYear ) ] ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Catch a owl:Class ;
  rdfs:label "Catch"@en ;
  iao:0000115 "The number of adult fishes that are caught in commercial, recreational, and First Nations fisheries."@en ; # definition
  iao:0000119 "Pacific Salmon Explorer Glossary; Harvest Catch Estimation Methods (DFO)."@en ; # definition source
  dcterms:source <https://www.salmonexplorer.ca/methods/glossary.html> , <https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/41263625.pdf> ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  rdfs:subClassOf iao:0000109. # measurement datum

gcdfos:COSEWIC a owl:Class ;
  rdfs:label "COSEWIC"@en ;
  iao:0000115 "The Committee on the Status of Endangered Wildlife in Canada (COSEWIC) is an independent advisory panel to the Minister of Environment and Climate Change Canada that meets twice a year to assess the status of wildlife species at risk of extinction. Members are wildlife biology experts from academia, government, non-governmental organizations and the private sector responsible for designating wildlife species in danger of disappearing from Canada."@en ; # definition
  rdfs:subClassOf dwc:Agent ;
  iao:0000119 "COSEWIC. (2003). COSEWIC - Committee on the Status of Endangered Wildlife in Canada. Available at cosewic.ca/index.php/en/ [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:DesignatableUnit a owl:Class ;
  rdfs:label "Designatable Unit"@en ;
  rdfs:subClassOf gcdfos:ReportingOrManagementStratum ;
  iao:0000115 "A unit of Canadian biodiversity that is discrete and evolutionarily significant, where discrete means that there is currently very little transmission of heritable (cultural or genetic) information from other such units, and evolutionarily significant means that the unit harbors heritable adaptive traits or an evolutionary history not found elsewhere in Canada."@en ; # definition
  iao:0000119 "COSEWIC. (2023). COSEWIC guidelines for recognizing designatable units. Available at https://cosewic.ca/index.php/en/reports/preparing-status-reports/guidelines-recognizing-designatable-units.html [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# COSEWIC Status Categories
#################################################################

gcdfos:COSEWICStatusCategory a skos:Concept ;
  skos:prefLabel "COSEWIC status category"@en ;
  skos:definition "The Committee on the Status of Endangered Wildlife in Canada (COSEWIC) assesses the national status of wild species, subspecies, varieties, or other designatable units that are considered to be at risk in Canada. Designations are made on native wildlife species including the following taxonomic groups: mammals, birds, reptiles, amphibians, fishes, arthropods, molluscs, vascular plants, mosses, and lichens."@en ;
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Extinct a gcdfos:COSEWICStatusCategory , skos:Concept ;
  skos:prefLabel "Extinct"@en ;
  skos:altLabel "X"@en ;
  skos:definition "A Wildlife Species that no longer exists."@en ;
  iao:0000115 "A COSEWIC status category applied to wildlife species that no longer exist."@en ; # definition
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Extirpated a gcdfos:COSEWICStatusCategory , skos:Concept ;
  skos:prefLabel "Extirpated"@en ;
  skos:altLabel "XT"@en ;
  skos:definition "A Wildlife Species that no longer exists in the wild in Canada, but exists elsewhere."@en ;
  iao:0000115 "A COSEWIC status category for wildlife species no longer existing in the wild in Canada but present elsewhere."@en ; # definition
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Endangered a gcdfos:COSEWICStatusCategory , skos:Concept ;
  skos:prefLabel "Endangered"@en ;
  skos:altLabel "E"@en ;
  skos:definition "A Wildlife Species facing imminent extirpation or extinction."@en ;
  iao:0000115 "A COSEWIC status category for wildlife species facing imminent extirpation or extinction."@en ; # definition
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Threatened a gcdfos:COSEWICStatusCategory , skos:Concept ;
  skos:prefLabel "Threatened"@en ;
  skos:altLabel "T"@en ;
  skos:definition "A Wildlife Species that is likely to become an endangered if nothing is done to reverse the factors leading to its extirpation or extinction."@en ;
  iao:0000115 "A COSEWIC status category for wildlife species likely to become endangered without intervention."@en ; # definition
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:SpecialConcern a gcdfos:COSEWICStatusCategory , skos:Concept ;
  skos:prefLabel "Special Concern"@en ;
  skos:altLabel "SC"@en ;
  skos:definition "A Wildlife Species that may become threatened or endangered because of a combination of biological characteristics and identified threats."@en ;
  iao:0000115 "A COSEWIC status category for wildlife species that may become threatened or endangered due to biology and identified threats."@en ; # definition
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:DataDeficient a gcdfos:COSEWICStatusCategory , skos:Concept ;
  skos:prefLabel "Data Deficient"@en ;
  skos:altLabel "DD"@en ;
  skos:definition "A category that applies when the available information is insufficient (a) to resolve a Wildlife Species' eligibility for assessment or (b) to permit an assessment of the Wildlife Species' risk of extinction."@en ;
  iao:0000115 "A COSEWIC status category used when information is insufficient to determine risk of extinction."@en ; # definition
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:NotAtRisk a gcdfos:COSEWICStatusCategory , skos:Concept ;
  skos:prefLabel "Not At Risk"@en ;
  skos:altLabel "NAR"@en ;
  skos:definition "A Wildlife Species that has been evaluated and found to be not at risk of extinction given the current circumstances."@en ;
  iao:0000115 "A COSEWIC status category for wildlife species evaluated and found not to be at risk of extinction."@en ; # definition
  skos:inScheme gcdfos:COSEWICStatusScheme ;
  iao:0000119 "COSEWIC. (2021). COSEWIC - Status categories. Available at https://cosewic.ca/index.php/en/assessment-process/cosewic-assessment-process-categories-and-guidelines/status-categories.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ConservationUnit a owl:Class ;
  rdfs:label "Conservation Unit"@en ;
  iao:0000115 "A group of fish sufficiently isolated from other groups that, if extirpated, is very unlikely to recolonize naturally within an acceptable timeframe."@en ; # definition
  iao:0000119 "Canada's Policy for Conservation of Wild Pacific Salmon Glossary."@en ; # definition source
  dcterms:source <https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/wsp-pss/policy-politique-eng.html#glossary> ;
  rdfs:subClassOf gcdfos:ReportingOrManagementStratum ;
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CUStatusBasedLRP a owl:Class ;
  rdfs:label "Conservation Unit status-based limit reference point"@en ;
  iao:0000115 "Stock Management Unit (SMU)-level limit reference points (LRPs) based on the status of component Wild Salmon Policy (WSP) Conservation Units (CUs) recommended to meet requirements of the Fisheries Act."@en ; # definition
  rdfs:subClassOf gcdfos:ReferencePoint ;
  iao:0000119 "DFO. (2022). Methodologies and Guidelines for Defining Limit Reference Points for Pacific Salmon. DFO Can. Sci. Advis. Sec. Sci. Advis. Rep. 2022/030. (Erratum: December 2022)"@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CUBenchmark a skos:Concept ;
  skos:prefLabel "Conservation Unit benchmark"@en ;
  skos:inScheme gcdfos:BiologicalBenchmarkScheme ;
  skos:definition "Identify biological production status under the Wild Salmon Policy to inform management and conservation decisions."@en ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CULowerBenchmark a gcdfos:CUBenchmark , skos:Concept ;
  skos:prefLabel "Conservation Unit lower benchmark"@en ;
  skos:definition "A reference point in biological status associated with significant losses in production between the Amber and Red zones, and which allows for a substantial buffer between it and any level of abundance that could lead to a CU being considered at risk of extinction."@en ;
  skos:inScheme gcdfos:BiologicalBenchmarkScheme ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CUHigherBenchmark a gcdfos:CUBenchmark , skos:Concept ;
  skos:prefLabel "Conservation Unit higher benchmark"@en ;
  skos:definition "A reference point in biological status to identify whether harvests are greater or less than the level expected to provide (on an average annual basis), the maximum annual catch for a CU, given existing environmental conditions."@en ;
  skos:inScheme gcdfos:BiologicalBenchmarkScheme ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Dataset a owl:Class ;
  rdfs:label "Dataset"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "A data product registered in catalogues (e.g., GeoNetwork); aligns with schema:Dataset."@en ; # definition
  skos:exactMatch schema:Dataset ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Enhancement a owl:Class ;
  rdfs:label "Enhancement"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "The application of biological and technical knowledge and capabilities to increase the productivity of fish stocks. It may be achieved by altering habitat attributes (e.g., habitat restoration) or by using fish culture techniques (e.g., hatcheries, spawning channels). In the context of the Wild Salmon Policy, only salmon originating from hatcheries and managed spawning channels will be considered enhanced."@en ; # definition
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Escapement a owl:Class ;
  rdfs:label "Escapement"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "The process by which mature salmon pass through (or escape) fisheries and return to fresh water to spawn."@en ; # definition
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:EscapementMeasurement a owl:Class ;
  rdfs:label "Escapement measurement"@en ;
  iao:0000115 "A measurement or estimate of escapement tied to a stock and a survey event."@en ; # definition
  rdfs:subClassOf iao:0000109 ; # measurement datum
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  skos:altLabel "Salmon escapement count"@en ;
  rdfs:seeAlso odo:SALMON_00000479 .

gcdfos:EquilibriumTradeoffAnalysis a owl:Class ;
  rdfs:label "Equilibrium trade-off analysis"@en ;
  rdfs:subClassOf gcdfos:Model ;
  iao:0000115 "Calculate summary performance measures across stocks (e.g., proportion of stocks meeting an objective vs. aggregate harvest, harvest rate, or escapement), under equilibrium conditions (i.e., on average over the long-term)."@en ; # definition
  iao:0000119 "DFO. (2023). Biological Benchmarks And Building Blocks For Aggregate-Level Management Targets For Skeena And Nass Sockeye Salmon (Oncorhynchus nerka). DFO Can. Sci. Advis. Sec. Sci. Advis. Rep. 2023/008."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ExploitationRate a owl:Class ;
  rdfs:label "Exploitation rate"@en ;
  iao:0000115 "Expressed as a percentage, the proportion of the total return of adult salmon in a given year that die (harvested or removed) as a result of fishing activity. ER = total fishing mortality / (total fishing mortality + escapement)"@en ; # definition
  rdfs:subClassOf gcdfos:TargetOrLimitRateOrAbundance ;
  iao:0000119 "Fisheries and Oceans Canada. (2019). Government of Canada. Pacific Salmon - Glossary. https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html. [Accessed 09-10-2025] "@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:TotalExploitationRate a owl:Class ;
  rdfs:label "Total exploitation rate"@en ;
  rdfs:subClassOf gcdfos:ExploitationRate ;
  iao:0000115 "The overall proportion of the total return of adult salmon removed by all fisheries over a defined period, aggregating across gear or fishery components."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Fecundity a owl:Class ;
  rdfs:label "Fecundity"@en ;
  rdfs:subClassOf bfo:0000020 ; # specifically dependent continuant
  iao:0000115 "The number of eggs an animal produces during each reproductive cycle; the potential reproductive capacity of an organism or population (similar to fertility)."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:FisheryReferencePoint a owl:Class , skos:Concept ;
  rdfs:label "Fishery reference point"@en ;
  skos:prefLabel "Fishery reference point"@en ;
  skos:inScheme gcdfos:BiologicalBenchmarkScheme ;
  iao:0000115 "A benchmark against which the biomass or abundance of the stock or the fishing mortality rate (or exploitation rate) can be measured in order to determine its status. These reference points can be targets, thresholds or limits depending on their intended use."@en ; # definition
  rdfs:subClassOf gcdfos:ReferencePoint ;
  dcterms:isPartOf <urn:ISBN:9780660401126> ;
  iao:0000119 "Marentette, J. R., Kronlund, A. R., & Cogliati, K. M. (2021). Specification of Precautionary Approach Reference Points and Harvest Control Rules in Domestically Managed and Assessed Key Harvested Stocks In Canada. DFO Can. Sci. Advis. Sec. Res. Doc. 2021/057. vii + 98 p."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:FishingEffort a owl:Class ;
  rdfs:label "Fishing effort"@en ;
  rdfs:subClassOf iao:0000109 ; # measurement datum
  iao:0000115 "A measure of fishing activity using metrics such as: the number of nets or lines used, the number of fishing vessels deployed, and the number of hours spent fishing."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:FishingMortality a owl:Class ;
  rdfs:label "Fishing mortality"@en ;
  iao:0000115 "Measure of deaths in a stock due to all fishing activities, including: targeted fishing, bycatch and discards, other incidental mortality in other fisheries."@en ; # definition
  rdfs:subClassOf gcdfos:ObservedRateOrAbundance ;
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:GeneticDiversity a owl:Class ;
  rdfs:label "Genetic diversity"@en ;
  rdfs:subClassOf bfo:0000020 ; # specifically dependent continuant
  iao:0000115 "The variation at the level of individual genes, and provides a mechanism for populations to adapt to their ever-changing environment. It refers to the differences in genetic make-up between distinct species and to genetic variations within a single species."@en ; # definition
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:HarvestAdvice a owl:Class ;
  rdfs:label "Harvest advice"@en ;
  rdfs:subClassOf iao:0000033 ; # directive information entity
  iao:0000115 "The scientific recommendation provided to fisheries managers on allowable levels of fish removals, derived from stock assessments, harvest control rules, and reference points. It reflects both the biological status of the stock and practical considerations such as ecosystem impacts, management measures, and market conditions. Harvest advice does not prescribe that the full allowable harvest must be taken, but rather defines the levels of harvest that are consistent with sustaining stock productivity and avoiding limit reference points."@en ; # definition
  dcterms:identifier <info:doi/10.1016/j.fishres.2018.04.007> ;
  iao:0000119 "Forrest, R. E., Holt, K. R., & Kronlund, A. R. (2018). Performance of alternative harvest control rules for two Pacific groundfish stocks with uncertain natural mortality: Bias, robustness and trade-offs. Fisheries Research, 206, p. 259-286."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Hatchery a owl:Class ;
  rdfs:label "Hatchery"@en ;
  rdfs:subClassOf dwc:MaterialEntity ;
  iao:0000115 "A hatchery is a facility where eggs are hatched under artificial conditions."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:HatcheryEnhancement a owl:Class ;
  rdfs:label "Hatchery enhancement"@en ;
  rdfs:subClassOf gcdfos:Enhancement ;
  iao:0000115 "The application of biological and technical knowledge and capabilities to increase the productivity of fish stocks originating from hatchery production."@en ; # definition
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:HatcheryOrigin a skos:Concept ;
  rdfs:label "Hatchery-origin"@en ;
  skos:inScheme gcdfos:SalmonOriginScheme ;
  iao:0000115 "Refers to broodstock fish that were born or reared in a hatchery and later selected for breeding purposes within the hatchery system."@en ; # definition
  iao:0000119 "DFO. (2018). Review of genetically based targets for enhanced contributions to Canadian Pacific Chinook Salmon populations. DFO Can. Sci. Advis. Sec. Sci. Advis. Rep. 2018/001. (Erratum: October 2023)"@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:HatcheryProduction a owl:Class ;
  rdfs:label "Hatchery production"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "Production of broodstock fish within the hatchery system. The production planning process considers scientific information, Indigenous knowledge, hatchery capacity, and risk assessments to determine the type and number of fish to produce in hatcheries, as well as where and when to release the fish."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2023). Production planning. Available at https://www.pac.dfo-mpo.gc.ca/sep-pmvs/hatcheries-ecloseries/production-eng.html [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:IndexBased a owl:Class ;
  rdfs:label "Index-based"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "Index-based management procedures (MPs), in general, adjust the catch based on changes in a population index over time."@en ; # definition
  iao:0000119 "Huynh, Q.C., Siegle, M.R., and Haggarty, D.R. 2025. Application of the Management Procedure Framework for Outside Quillback Rockfsh (Sebastes maliger ) in British Columbia in 2021. DFO Can. Sci. Advis. Sec. Res. Doc. 2024/028. iv + 201 p."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Indicator a owl:Class ;
  rdfs:label "Indicator"@en ;
  rdfs:subClassOf gcdfos:IndexBased ;
  iao:0000115 "A derived assessment index used in Fisheries Science Reports or stock status reporting."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:IndicatorPopulation a owl:Class ;
  rdfs:label "Indicator population"@en ;
  rdfs:subClassOf dwc:Organism ;
  iao:0000115 "Indicator populations are directly assessed and deemed representative of other populations in the area that are not directly assessed. The concept of using indicator populations is based on an expectation of biological homogeneity over the geographic region represented by the indicator population. Often, indicator populations are hatchery-supplemented and only a portion of the hatchery-origin component of the population is tagged, and is assumed to be representative of the entire population, both of hatchery origin and natural origin. The indicator population is expected to be representative, in terms of life history, marine distribution, productivity, and exploitation pattern, of other populations within the indicator region."@en ; # definition
  dcterms:indicator <info:doi/10.1002/ece3.6383> ;
  iao:0000119 "Beacham, T. D., Wallace, C., Jonsen, K., McIntosh, B., Candy, J. R., Willis, D., Lynch, C., & Withler, R. E. (2020). Insights on the concept of indicator populations derived from parentage-based tagging in a large-scale coho salmon application in British Columbia, Canada. Ecology and Evolution, 10, p 6461-6476."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:IndicatorRiver a owl:Class ;
  rdfs:label "Indicator river"@en ;
  skos:altLabel "Indicator stream"@en ;
  rdfs:subClassOf envo:01000618 ; # lotic water body
  iao:0000115 "All spawning streams that are listed to be monitored by DFO are classified as an indicator or non-indicator stream. Indicator streams are those that have been identified in some Regions as providing more reliable indices of spawner abundance. Indicator streams tend to give relatively accurate annual spawner abundance estimates, given that they use higher quality methodologies and are more intensively/regularly surveyed. Indicator streams are also assumed to be representative of spawner returns to other streams in close geographic proximity. Non-indicator streams are typically less consistently surveyed, using more variable methodologies, and/or may simply be difficult to survey (e.g., poor water quality, remote location, etc.)."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:GraphicalVisualization a owl:Class ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  rdfs:label "Graphical visualization"@en ;
  iao:0000115 "An information content entity that visually represents data or model outputs, including plots, maps, and diagrams."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:KobePlot a owl:Class ;
  rdfs:label "Kobe plot"@en ;
  rdfs:subClassOf gcdfos:GraphicalVisualization ;
  iao:0000115 "A visual way to show the status of a stock. The plot is divided into four panels which correspond to a particular condition of the stock (overfishing, underfishing, over-fished, and underfished). By developing a ratio of how much fishing is occurring (F) versus the estimated number of fish (B) for any particular fishery, a point can be plotted in one of those four panels, thus telling the status."@en ; # definition
  iao:0000119 "Western Pacific Regional Fishery Management Council. (2020). Science & Management 101: What is a Kobe Plot? Available at https://www.wpcouncil.org/wp-content/uploads/2020/06/What-is-a-Kobe-plot.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Model a owl:Class ;
  rdfs:label "Model"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  iao:0000115 "A tool used to simplify a natural system so it can be more easily understood."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 10-28-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ModelExecution a owl:Class ;
  rdfs:label "Model execution"@en ;
  iao:0000115 "The implementation of a model using a specific set of input data to generate output data."@en ; # definition
  rdfs:subClassOf dwc:Event ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:LifeCycleModel a owl:Class ;
  rdfs:label "Life cycle model"@en ;
  rdfs:subClassOf gcdfos:Model ;
  iao:0000115 "The life cycle of a typical fish species can be used as a template for considering the assessment of project-induced impacts on fisheries productivity. Fisheries productivity results from individual fish completing their life cycle, and having vital rates (reproduction, growth, survival, etc.) that are sufficient to generate a sustainable yield at the population level. "@en ; # definition
  iao:0000119 "Bradford, M. J., Randall, R. G., Smokorowski, K. S., Keatley, B. E., & Clarke, K. D. (2014). A framework for assessing fisheries productivity for the Fisheries Protection Program. DFO Can. Sci. Advis. Sec. Res. Doc. 2013/067. v + 25 p."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:LimitReferencePoint a owl:Class ;
  rdfs:label "Limit reference point"@en ;
  skos:altLabel "LRP"@en ;
  iao:0000115 "A fish stock experiencing serious harm may be depleted to the point where it has impaired productivity or reproductive capacity. As a result, the stock would be less resilient to fishing and could not easily rebuild. Serious harm may be irreversible, or only slowly reversible over the long-term, and can be due to fishing, other human-induced impacts, or natural causes. Under the Precautionary Approach policy and the Fish Stocks Provisions (FSPs), breaching the LRP is a trigger for a rebuilding plan."@en ; # definition
  rdfs:subClassOf gcdfos:ReferencePoint ;
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:MarineSurvival a owl:Class ;
  rdfs:label "Marine survival"@en ;
  iao:0000115 "Marine survival is defined as the portion of the coded-wire tagged smolt release that has survived to be either caught in marine fisheries or returned to freshwater as adult."@en ; # definition
  rdfs:subClassOf gcdfos:Indicator ;
  iao:0000119 "Fisheries and Oceans Canada. (2025). 2024 Marine Survival Forecast of Southern British Columbia Coho. 17 p."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:MixedStockFishery a owl:Class ;
  rdfs:label "Mixed-stock fishery"@en ;
  rdfs:subClassOf odo:SALMON_00000137 ;
  iao:0000115 "Individual fish in different stocks might mix or migrate between different areas. Also referred to as 'multispecies fishery'."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment. Available at https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/module-1/03-eng.html [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:MaximumSustainableYield a owl:Class ;
  rdfs:label "Maximum sustainable yield"@en ;
  skos:altLabel "MSY"@en ;
  iao:0000115 "The largest catch (yield) that can be taken on average from a population under existing environmental conditions. Catch will vary annually due to variation in a populations survival rate."@en ; # definition
  rdfs:subClassOf gcdfos:TargetOrLimitRateOrAbundance ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:NaturalOrigin a skos:Concept ;
  rdfs:label "Natural-origin"@en ;
  skos:prefLabel "Natural-origin"@en ;
  skos:inScheme gcdfos:SalmonOriginScheme ;
  iao:0000115 "Individuals that are born and reared in the wild (natural environment) but are later collected and used in breeding programs."@en ; # definition
  iao:0000119 "DFO. (2018). Review of genetically based targets for enhanced contributions to Canadian Pacific Chinook Salmon populations. DFO Can. Sci. Advis. Sec. Sci. Advis. Rep. 2018/001. (Erratum: October 2023)"@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:OceanEntryYear a owl:Class ;
  rdfs:label "Ocean entry year"@en ;
  iao:0000115 "Ocean entry year (OEY) refers to the year in which salmon are released from the hatchery."@en ; # definition
  rdfs:subClassOf odo:ECSO_00002050 ; #year of measurement; brood year is also a subclass
  iao:0000119 "Pacific Salmon Foundation. (2022). A Review of Hatchery Release Strategies. Available at https://www.marinescience.ca/wp-content/uploads/2022/03/2022PDF-ReleaseStrategiesReview-Newsletter-Web.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:OperationalControlPoint a owl:Class ;
  rdfs:label "Operational control point"@en ;
  rdfs:subClassOf iao:0000030 ; # information content entity
  skos:altLabel "OCP"@en ;
  iao:0000115 "A value of an indicator or other input variable that acts as a trigger for a change in management actions, as for example in a Harvest Control Rule."@en ; # definition
  dcterms:isPartOf <urn:ISBN:9780660401126> ;
  iao:0000119 "Marentette, J. R., Kronlund, A. R., & Cogliati, K. M. (2021). Specification of Precautionary Approach Reference Points and Harvest Control Rules in Domestically Managed and Assessed Key Harvested Stocks In Canada. DFO Can. Sci. Advis. Sec. Res. Doc. 2021/057. vii + 98 p."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:PercentHatcheryOriginSpawners a owl:Class ;
  rdfs:label "Percent of hatchery origin spawners"@en ;
  rdfs:subClassOf gcdfos:ObservedRateOrAbundance ;
  iao:0000115 "Percent of salmon spawning in the natural environment that were reared in a hatchery and are first generation hatchery fish. Contrast with Natural origin spawners (not reared in a hatchery, but could be second generation hatchery) or wild salmon with no hatchery lineage."@en ; # definition
  iao:0000119 "Pacific Marine Conservation Caucus, Salmon Subcommittee. (2022). Mark Selective Fisheries and Mass Marking under the Pacific Salmon Strategy Initiative: Comments and Feedback on Proposed Expansion. Pacific Marine Conservation Caucus Salmon Subcommittee. Available at https://www.mccpacific.org/wp-content/uploads/2022/10/MCC-Salmon-Subcommittee-Comments-on-DFOs-MM-and-MSF-Discussion-Paper-220930.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:PreTerminalFishery a owl:Class ;
  rdfs:label "Pre-terminal fishery"@en ;
  iao:0000115 "Pre-terminal fisheries occur outside the river and areas immediately adjacent to the river systems."@en ; # definition
  rdfs:subClassOf odo:SALMON_00000137 ; #fishery type
  iao:0000119 "National Oceanic and Atmospheric Administration. (2023). Endangered Species Act Section 7(a)(2) Biological Opinion and Magnuson-Stevens Fishery Conservation and Management Act Essential Fish Habitat Response for the Continued Operation and Maintenance of the Willamette Valley System. Available at https://www.fisheries.noaa.gov/s3//2025-05/wcro-2023-00324-willamette-valley-system-20241226.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:PrecautionaryApproach a gcdfos:PolicyFramework , iao:0000033 ; # directive | directive information entity
  rdfs:label "Precautionary Approach"@en ;
  skos:altLabel "PA"@en ;
  iao:0000115 "When used in an advisory context in support of decision-making by the Government of Canada, this term conveys the sense that the advice is provided in situations of high scientific uncertainty. It is intended to promote actions that would result in a low probability of harm that is serious or difficult to reverse."@en ; # definition
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RebuildingPlan a gcdfos:PolicyFramework , iao:0000033 ; # directive | directive information entity
  rdfs:label "Rebuilding plan"@en ;
  iao:0000115 "Rebuilding plans are standalone documents specifically intended to address statutory requirements under the 2019 amendments to the Fisheries Act for the prescribed major fish stock subject to the Fish Stocks provisions (FSP). The obligation to develop and implement a rebuilding plan is triggered when a stock declines to, or below, its LRP. A rebuilding plan for a prescribed major fish stock must be developed within 24 months of the day on which the Minister first has knowledge the stock has declined to or below its LRP. If a stock is at or below its LRP when it is prescribed under the FSP, the 24-month timeline to develop a rebuilding plan for the stock starts the day the stock is prescribed in regulation. The approved plan must be published on DFO’s website. The plan should be published within 120 days from the date of the plan’s approval by the Minister."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2022). Guidelines for writing rebuilding plans per the Fish Stocks Provisions and A Fishery Decision-making Framework Incorporating the Precautionary Approach. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/precautionary-precaution-eng.htm. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RebuildingTarget a owl:Class ;
  rdfs:label "Rebuilding target"@en ;
  rdfs:subClassOf iao:0000033 ; # directive information entity
  iao:0000115 "The rebuilding target for the stock must, at a minimum, be set at a level above the LRP such that there is a very low to low likelihood of the stock being below the LRP (<5-25% probability; as defined in Annex 2B of the 2009 Precautionary Approach Policy). A stock’s rebuilding plan will remain in effect until the stock reaches its rebuilding target."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2022). Guidelines for writing rebuilding plans per the Fish Stocks Provisions and A Fishery Decision-making Framework Incorporating the Precautionary Approach. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/precautionary-precaution-eng.htm. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Recruitment a owl:Class ;
  rdfs:label "Recruitment"@en ;
  rdfs:subClassOf bfo:0000020 ; # specifically dependent continuant
  iao:0000115 "The individual juveniles that survive and are added to a population. Recruitment in the context of salmon management usually refers to the abundance of adults before fishing. Recruitment is typically calculated as the sum of escapement, all fisheries catches, and estimated pre-spawn and post-release mortality."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ReferencePoint a owl:Class ;
  rdfs:label "Reference point"@en ;
  iao:0000115 "Reference points are used as targets to achieve or as limits to avoid."@en ; # definition
  iao:0000119 "DFO Introduction to Stock Assessment Glossary."@en ; # definition source
  dcterms:source <https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html> ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  rdfs:subClassOf iao:0000030 . # Information content entity.

gcdfos:RemovalRate a owl:Class ;
  rdfs:label "Removal rate"@en ;
  rdfs:subClassOf iao:0000109 ; # measurement datum
  iao:0000115 "The ratio of all human induced removals and total exploitable stock size."@en ; # definition
  dcterms:identifier <info:doi/10.1016/j.fishres.2018.04.007> ;
  iao:0000119 "Forrest, R. E., Holt, K. R., & Kronlund, A. R. (2018). Performance of alternative harvest control rules for two Pacific groundfish stocks with uncertain natural mortality: Bias, robustness and trade-offs. Fisheries Research, 206, p. 259-286."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  rdfs:subClassOf gcdfos:Indicator .

gcdfos:RemovalReference a owl:Class ;
  rdfs:label "Removal reference"@en ;
  rdfs:subClassOf gcdfos:TargetOrLimitRateOrAbundance ;
  skos:altLabel "RR"@en ;
  iao:0000115 "Removal reference is the maximum removal rate of a fish stock. It is less than or equal to the stock's maximum sustainable yield. It can be expressed in terms of fishing mortality, harvest rate, and fishing effort."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RunReconstructionModel a owl:Class ;
  rdfs:label "Run reconstruction model"@en ;
  rdfs:subClassOf gcdfos:Model ;
  iao:0000115 "Run reconstruction models use escapement data, catch data, and knowledge of migratory paths and timing to calculate stock contribution and harvest rates."@en ; # definition
  dcterms:identifier <info:doi/10.1139/f88-258> ;
  iao:0000119 "Starr, P. & Hilborn, R. (1988). Reconstruction of Harvest Rates and Stock Contribution in Gauntlet Salmon Fisheries: Application to British Columbia and Washington Sockeye (Oncorhynchus nerka). Can. J. Fish. Aqu. Sci., 45, p 2054-2246."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:SizeAtAge a owl:Class ;
  rdfs:label "Size-at-age"@en ;
  iao:0000115 "The size of an individual at a given age."@en ; # definition
  dcterms:identifier <info:doi/10.1111/gcb.70296> ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  rdfs:subClassOf odo:SALMON_00000167 . # fish measurement type

gcdfos:Sgen a owl:Class ;
  rdfs:label "Spawning biomass in one generation"@en ;
  rdfs:subClassOf gcdfos:TargetOrLimitRateOrAbundance ;
  iao:0000115 "Spawner abundance that will result in recovery to SMSY in one generation in the absence of fishing under equilibrium conditions."@en ; # definition
  iao:0000119 "Holt, C. A. (2009). Evaluation of benchmarks for conservation units in Canada’s Wild Salmon Policy: Technical Documentation. DFO Can. Sci. Advis. Sec. Res. Doc. 2009/059. x + 50 p."@en ; # definition source
  skos:altLabel "Sgen"@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:SMSY a owl:Class ;
  rdfs:label "Spawning biomass at Maximum Sustainable Yield"@en ;
  rdfs:subClassOf gcdfos:TargetOrLimitRateOrAbundance ;
  iao:0000115 "The predicted number of spawners producing maximum sustained yield. It is a key biological reference point used in fisheries management and benchmark under the Wild Salmon Policy to maintain fish populations at sustainable levels."@en ; # definition
  dcterms:isPartOf <urn:ISSN:1499-3848> ;
  iao:0000119 "Parken, C. K., McNicol, R. E., & Irvine, J. R. (2006). Habitat-based methods to estimate escapement goals for data limited Chinook salmon stocks in British Columbia, 2004. Canadian Science Advisory Secretariat Research Document 2006/083. Ottawa, Ontario, Canada."@en ; # definition source
  skos:altLabel "SMSY"@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:SpawnerAbundance a owl:Class ;
  rdfs:label "Spawner abundance"@en ;
  rdfs:subClassOf iao:0000109 ; # measurement datum
  iao:0000115 "Spawner abundance (also called “escapement”) provides information on the number of salmon that “escape” fisheries and make it back to spawn."@en ; # definition
  iao:0000119 "Pacific Salmon Foundation. (2025). State of Salmon 2025 Report. Pacific Salmon Foundation, Vancouver, BC, Canada."@en; # definition source
  dcterms:identifier <info:doi/10.60740/stateofsalmon.2025> ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  skos:closeMatch odo:SALMON_00000479 ;
  rdfs:comment "Salmon escapement count"@en .

gcdfos:Stock a owl:Class ;
  rdfs:label "Stock"@en ;
  iao:0000115 "A biologically meaningful but operationally-focused salmon demographic unit used in assessment and reporting."@en ; # definition
  iao:0000119 "DFO Introduction to Stock Assessment Glossary; Pacific Salmon Commission Technical Report No. 49."@en ; # definition source
  dcterms:source <https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html> ;
  rdfs:subClassOf dwc:Organism ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
  skos:altLabel "Fish stock type"@en ;
  rdfs:seeAlso odo:SALMON_00000639 .

gcdfos:StockAssessment a owl:Class ;
  rdfs:label "Stock assessment"@en ;
  rdfs:subClassOf bfo:0000015 ; # process
  iao:0000115 "The scientific process of analyzing data to estimate the abundance and productivity of fish stocks in the past, present, and future. It is the backbone of successful science-based fisheries management."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment. Available at https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/module-1/09-eng.html [Accessed 10-28-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:StockManagementUnit a owl:Class ;
  rdfs:label "Stock Management Unit"@en ;
  iao:0000115 "A collection of stocks that are managed together with a joint stock status."@en ; # definition
  iao:0000119 "DFO Introduction to Stock Assessment Glossary."@en ; # definition source
  dcterms:source <https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html> ;
  rdfs:subClassOf gcdfos:ReportingOrManagementStratum ;
  skos:altLabel "SMU"@en ;
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:StockRecruitmentRelationship a owl:Class ;
  rdfs:label "Stock-recruitment relationship"@en ;
  rdfs:subClassOf gcdfos:Model ;
  iao:0000115 "The relationships between the abundance or biomass of spawning adults in a stock and the expected number of recruits they can produce each year."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to stock assessment. Available at https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/module-1/10-eng.html [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:StockStatusZone a skos:Concept ;
  skos:prefLabel "Stock status zone"@en ;
  skos:definition "Categorical assessment of the biological status of a fish stock under the Precautionary Approach framework (Healthy, Cautious, Critical, Status uncertain) according to upper stock and limit reference points."@en ;
  skos:inScheme gcdfos:StockStatusZoneScheme ;
  iao:0000119 "Fisheries and Oceans Canada. (2009). A fishery decision-making framework incorporating the precautionary approach. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/precaution-eng.htm. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CriticalZone a gcdfos:StockStatusZone , skos:Concept ;
  skos:prefLabel "Critical zone"@en ;
  skos:definition "In the critical zone, stock growth is promoted and removals are kept to the lowest possible level. The stock is considered to be in “the critical zone” if the mature biomass, or its index, is less than or equal to 40% of BMSY. In other words:  Biomass ≤ 40% BMSY."@en ;
  iao:0000119 "Fisheries and Oceans Canada. (2009). A fishery decision-making framework incorporating the precautionary approach. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/precaution-eng.htm. [Accessed 09-10-2025]."@en ; # definition source
  skos:inScheme gcdfos:StockStatusZoneScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CautiousZone a gcdfos:StockStatusZone , skos:Concept ;
  skos:prefLabel "Cautious zone"@en ;
  skos:definition "In the cautious zone, decisions and strategies promote stock rebuilding to the healthy zone. The stock is considered to be in the “cautious zone” if the biomass, or its index, is higher than 40% of BMSY but lower than 80% of BMSY. In other words:  40% BMSY < Biomass < 80% BMSY."@en ;
  iao:0000119 "Fisheries and Oceans Canada. (2009). A fishery decision-making framework incorporating the precautionary approach. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/precaution-eng.htm. [Accessed 09-10-2025]."@en ; # definition source
  skos:inScheme gcdfos:StockStatusZoneScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:HealthyZone a gcdfos:StockStatusZone , skos:Concept ;
  skos:prefLabel "Healthy zone"@en ;
  skos:definition "In the healthy zone, the fish stock status is good, and fisheries management decisions and harvest strategies are designed to maintain fish stocks within this zone. The stock is considered to be “healthy” if the biomass, or its index, is higher than 80% of BMSY. In other words:  Biomass ≥ 80% BMSY."@en ;
  iao:0000119 "Fisheries and Oceans Canada. (2009). A fishery decision-making framework incorporating the precautionary approach. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/precaution-eng.htm. [Accessed 09-10-2025]."@en ; # definition source
  skos:inScheme gcdfos:StockStatusZoneScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:UncertainStatus a gcdfos:StockStatusZone , skos:Concept ;
  skos:prefLabel "Uncertain status"@en ;
  skos:definition "For stocks that have an uncertain status, the uncertainty is factored into harvest level decisions. Greater uncertainty leads to more precautionary harvest levels."@en ;
  iao:0000119 "Fisheries and Oceans Canada. (2009). A fishery decision-making framework incorporating the precautionary approach. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/precaution-eng.htm. [Accessed 09-10-2025]."@en ; # definition source
  skos:inScheme gcdfos:StockStatusZoneScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:SurveyEvent a owl:Class ;
  rdfs:label "Survey event"@en ;
  iao:0000115 "A field event that collects information about key fish stocks to track their status, and track progress to implement fishery policies and measures for these stocks."@en ; # definition
  rdfs:subClassOf dwc:Event ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). About the Sustainability Survey for Fisheries. Available at https://www.dfo-mpo.gc.ca/reports-rapports/regs/sff-cpd/survey-sondage/about-propos-en.html. [Accessed 09-10-2025]."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:TargetReferencePoint a owl:Class ;
  rdfs:label "Target reference point"@en ;
  iao:0000115 "The overall stock level target. It is determined by setting productivity, broader ecological, and socio-economic objectives for the fishery. It is typically set at or above the USR."@en ; # definition
  rdfs:subClassOf gcdfos:ReferencePoint ;
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to stock assessment. Available at https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/module-1/11-eng.html [Accessed 09-10-2025]."@en ; # definition source
  skos:altLabel "TRP"@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:TerminalFishery a owl:Class ;
  rdfs:label "Terminal fishery"@en ;
  iao:0000115 "Fisheries that occur near or in fresh water (e.g., at a river’s mouth) where the targeted species or stock has returned to spawn."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:subClassOf odo:SALMON_00000137 ; #fishery type
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:TotalAllowableCatch a owl:Class ;
  rdfs:label "Total allowable catch"@en ;
  rdfs:subClassOf gcdfos:TargetOrLimitRateOrAbundance ;
  iao:0000115 "A policy-defined catch limit set for a particular fishery within a specified management period. Also referred to as quota."@en ; # definition
  iao:0000119 "Fisheries and Oceans Canada. (2024). Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html. [Accessed 09-10-2025]."@en ; # definition source
  skos:altLabel "TAC"@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:UMSY a owl:Class ;
  rdfs:label "Exploitation rate at Maximum Sustainable Yield"@en ;
  rdfs:subClassOf gcdfos:TargetOrLimitRateOrAbundance ;
  iao:0000115 "The exploitation rate associated with the maximum sustainable yield (MSY). Usually expressed as an annual proportion. Exploitation rate is synonymous with harvest rate (U)."@en ; # definition
  dcterms:isPartOf <urn:ISBN:9780660401126> ;
  iao:0000119 "Marentette, J. R., Kronlund, A. R., & Cogliati, K. M. (2021). Specification of Precautionary Approach Reference Points and Harvest Control Rules in Domestically Managed and Assessed Key Harvested Stocks In Canada. DFO Can. Sci. Advis. Sec. Res. Doc. 2021/057. vii + 98 p."@en ; # definition source
  skos:altLabel "UMSY"@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:UpperStockReferencePoint a owl:Class ;
  rdfs:label "Upper stock reference"@en ;
  iao:0000115 "A threshold below which fishing must be reduced to avoid reaching the LRP. The USR must be set at an appropriate distance above the LRP to provide sufficient opportunity for managers to recognize a declining stock status and sufficient time for management actions to take effect. "@en ; # definition
  rdfs:subClassOf gcdfos:ReferencePoint ;
  iao:0000119 "DFO Introduction to Stock Assessment Glossary."@en ; # definition source
  skos:altLabel "USR"@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Wild a owl:Class ;
  rdfs:label "Wild"@en ;
  rdfs:subClassOf dwc:Organism ;
  iao:0000115 "Individuals having spent their entire life cycle in the wild and originate from parents that were also produced by natural spawning and continuously lived in the wild."@en ; # definition
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:WildSalmonPolicy a gcdfos:PolicyFramework , iao:0000033 ; # directive information entity
  rdfs:label "Wild Salmon Policy"@en ;
  iao:0000115 "Canada's Policy for Conservation of Wild Pacific Salmon is a framework designed to restore and maintain healthy and diverse salmon populations and their habitats for the benefit of Canadians in perpetuity. It prioritizes conservation of wild salmon through identifying and managing conservation units (CUs) that reflect the genetic and geographic diversity of salmon populations, while incorporating habitat protection, ecosystem management, and sustainable fisheries practices."@en ; # definition
  dcterms:isPartOf <urn:ISBN:0662405382> ;
  iao:0000119 "Fisheries and Oceans Canada. (2005). Canada's Policy for Conservation of Wild Pacific Salmon. Retrieved from https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf."@en ; # definition source
  skos:altLabel "WSP"@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:WSPBiologicalStatusZone a skos:Concept ;
  skos:prefLabel "Wild Salmon Policy biological status zone"@en ;
  skos:definition "The biological status of a Conservation Unit (CU) will normally be based on the abundance and distribution of spawners in the unit, or proxies thereof. For each CU, higher and lower benchmarks will be defined that will delimit three status zones: Green, Amber, and Red. As spawner abundance decreases, a CU moves towards the lower status zone, and the extent of management intervention for conservation purposes will increase."@en ;
  rdfs:seeAlso gcdfos:WSPBiologicalStatusZoneScheme ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:GreenZone a gcdfos:WSPBiologicalStatusZone , skos:Concept ;
  skos:prefLabel "Green zone"@en ;
  skos:definition "Social and economic considerations will tend to be the primary drivers for the management of Conservation Units in the Green zone, though ecosystem or other non-consumptive use values could also be considered."@en ;
  skos:inScheme gcdfos:WSPBiologicalStatusZoneScheme ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:AmberZone a gcdfos:WSPBiologicalStatusZone , skos:Concept ;
  skos:prefLabel "Amber zone"@en ;
  skos:definition "Amber status implies caution in the management of the Conservation Unit. While a CU in the Amber zone should be at a low risk of loss, there will be a degree of lost production. Decisions about the conservation of CUs in the Amber zone will involve broader consideration of biological, social, and economic issues. "@en ;
  skos:inScheme gcdfos:WSPBiologicalStatusZoneScheme ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RedZone a gcdfos:WSPBiologicalStatusZone , skos:Concept ;
  skos:prefLabel "Red zone"@en ;
  skos:definition "A Conservation Unit in the Red zone is undesirable because of the risk of extirpation, and the loss of ecological benefits and salmon production. Changes in status will trigger management actions that will vary depending on species, geographic regions, and cause of the decline."@en ;
  skos:inScheme gcdfos:WSPBiologicalStatusZoneScheme ;
  iao:0000119 "Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:WSPIntegratedStatus a skos:Concept;
  skos:prefLabel "WSP integrated status"@en ;
  skos:definition "Since CU status evaluations can include more than one metric, it is possible that different metrics could each indicate a different WSP status zone. A final step that synthesizes all metric and status-related information into an integrated status for each CU, and provides expert commentary on this information, is necessary as inputs into subsequent implementation of WSP Strategy 4 (Integrated Strategic Planning) to prioritize assessment activities and management actions ."@en ;
  skos:inScheme gcdfos:WSPBiologicalStatusZoneScheme ;
  iao:0000119 "DFO. (2016). Integrated Biological Status of Southern British Columbia Chinook Salmon (Oncorhynchus tshawytscha) Under the Wild Salmon Policy. DFO Can. Sci. Advis. Sec. Sci. Advis. Rep. 2016/042"@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:WSPRapidStatus a skos:Concept;
  skos:prefLabel "WSP rapid status"@en ;
  skos:definition "The WSP rapid status assessment approach builds on previously completed WSP integrated status assessments. The detailed WSP integrated assessments are labour intensive, taking 10-40 experts up to three days to complete for each group of CUs. For this reason, assessments have only been completed for ~11% of the 377 current Pacific Salmon CUs, many of which are outdated. The WSP rapid approach can fill gaps by expanding coverage of CU statuses across time, species, and geographic areas. The WSP rapid status approach ensures statuses are scientifically objective, consistent, and comparable across BC/Yukon CUs. This approach is also relatively easy to implement, applicable to data rich and data poor CUs, and can be updated annually."@en ;
  skos:inScheme gcdfos:WSPBiologicalStatusZoneScheme ;
  iao:0000119 "Pestal, G., MacDonald, B. L., Grant, S. C. H., and Holt, C. A. (2023). State of the Salmon: rapid status assessment approach for Pacific salmon under Canada’s Wild Salmon Policy. Can. Tech. Rep. Fish. Aquat. Sci. 3570: xiv + 200 p."@en ; # definition source
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# FSAR triggers, requirements, and policy structures
#################################################################

gcdfos:TriggerCondition a owl:Class ;
  rdfs:label "Trigger condition"@en ;
  iao:0000115 "A general condition observed from assessment metadata (for example, a CU entering the Red zone) that activates one or more policy or reporting requirements."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Requirement a owl:Class ;
  rdfs:label "Requirement"@en ;
  iao:0000115 "A high-level obligation or check that applies when associated triggers hold (for example, summarizing rebuilding actions when a CU is below its limit reference point)."@en ; # definition
  rdfs:subClassOf iao:0000104 ; # plan specification
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:PolicyFramework a owl:Class ;
  rdfs:label "Policy framework"@en ;
  iao:0000115 "A legal or policy framework that mandates or informs fisheries management obligations (for example, the Wild Salmon Policy or the Precautionary Approach)."@en ; # definition
  rdfs:subClassOf iao:0000104 ; # plan specification
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:BelowLRPTrigger a owl:Class ;
  rdfs:label "Below LRP trigger"@en ;
  iao:0000115 "Trigger condition that holds when a CU is at or below its limit reference point (typically associated with a Red rapid status)."@en ; # definition
  rdfs:subClassOf gcdfos:TriggerCondition ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:BetweenLRPUSRTrigger a owl:Class ;
  rdfs:label "Between LRP and USR trigger"@en ;
  iao:0000115 "Trigger condition that holds when a CU lies between its limit and upper stock reference points (typically associated with an Amber rapid status)."@en ; # definition
  rdfs:subClassOf gcdfos:TriggerCondition ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:UncertaintyMissingConfidenceTrigger a owl:Class ;
  rdfs:label "Missing confidence trigger"@en ;
  iao:0000115 "Trigger condition that flags missing or incomplete confidence information for a rapid status assessment."@en ; # definition
  rdfs:subClassOf gcdfos:TriggerCondition ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:MixedStockNoGSITrigger a owl:Class ;
  rdfs:label "Mixed-stock without GSI trigger"@en ;
  iao:0000115 "Trigger condition indicating that a mixed-stock fishery does not use genetic stock identification to apportion catch."@en ; # definition
  rdfs:subClassOf gcdfos:TriggerCondition ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CUProxiedTrigger a owl:Class ;
  rdfs:label "CU proxied trigger"@en ;
  iao:0000115 "Trigger condition indicating that one or more conservation units were excluded or proxied in the assessment."@en ; # definition
  rdfs:subClassOf gcdfos:TriggerCondition ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RebuildingPlanRequirement a owl:Class ;
  rdfs:label "Rebuilding plan requirement"@en ;
  iao:0000115 "Requirement to summarize rebuilding obligations and reference the relevant plan when a CU is at or below its limit reference point."@en ; # definition
  rdfs:subClassOf gcdfos:Requirement ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CautiousHarvestReviewRequirement a owl:Class ;
  rdfs:label "Cautious zone harvest requirement"@en ;
  iao:0000115 "Requirement to document precautionary harvest measures when a CU resides between the limit and upper stock reference points."@en ; # definition
  rdfs:subClassOf gcdfos:Requirement ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:UncertaintyDisclosureRequirement a owl:Class ;
  rdfs:label "Uncertainty disclosure requirement"@en ;
  iao:0000115 "Requirement to record and communicate uncertainty considerations or data gaps associated with an assessment."@en ; # definition
  rdfs:subClassOf gcdfos:Requirement ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:MixedStockJustificationRequirement a owl:Class ;
  rdfs:label "Mixed-stock justification requirement"@en ;
  iao:0000115 "Requirement to justify the absence of genetic stock identification or explain apportionment methods for mixed-stock fisheries."@en ; # definition
  rdfs:subClassOf gcdfos:Requirement ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CUAccountingNoteRequirement a owl:Class ;
  rdfs:label "CU accounting note requirement"@en ;
  iao:0000115 "Requirement to document how conservation units were included, excluded, or proxied during an assessment."@en ; # definition
  rdfs:subClassOf gcdfos:Requirement ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Condition a owl:Class ;
  rdfs:label "Decision condition"@en ;
  iao:0000115 "A condition entity used to relate rapid-status results or metric thresholds to downstream decision requirements."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:DecisionRequirement a owl:Class ;
  rdfs:label "Decision requirement"@en ;
  iao:0000115 "A reusable pattern describing management or policy obligations that are triggered by specific assessment conditions."@en ; # definition
  rdfs:subClassOf iao:0000104 ; # plan specification
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:PolicyReadiness a owl:Class ;
  rdfs:label "Policy and legal readiness"@en ;
  iao:0000115 "Summarizes whether an assessment satisfies mandatory policy gates such as WSP rapid status disclosure, harvest control rule identification, and Fish Stocks provisions compliance."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ManagementDecision a owl:Class ;
  rdfs:label "Management decision"@en ;
  iao:0000115 "A fisheries management action informed by scientific advice, such as establishing a total allowable catch or implementing time-area closures."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:CUInclusion a owl:Class ;
  rdfs:label "CU accounting entry"@en ;
  iao:0000115 "Records how a specific conservation unit is treated within an assessment cycle—included, excluded, or proxied—with supporting rationale and review metadata."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:GSIUsage a owl:Class ;
  rdfs:label "Genetic stock identification usage"@en ;
  iao:0000115 "Captures whether and how genetic stock identification data were used to support a mixed-stock assessment, including sample size and uncertainty details."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ChangeLogEntry a owl:Class ;
  rdfs:label "Advice change log entry"@en ;
  iao:0000115 "Documents a change from the previous assessment cycle, such as data updates, methodological adjustments, or altered advice."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:WSPMetric a owl:Class ;
  rdfs:label "WSP metric"@en ;
  iao:0000115 "One of the standard Wild Salmon Policy rapid-status metrics (absolute abundance, relative abundance, long-term trend, or percent change)."@en ; # definition
  iao:0000119 "Pestal, G. et al. (2023). State of the Salmon: Rapid Status Assessment Approach for Pacific Salmon under Canada’s Wild Salmon Policy."@en ; # definition source
  dcterms:source <https://publications.gc.ca/site/eng/9.928944/publication.html> ;
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:MetricBenchmark a owl:Class ;
  rdfs:label "Metric benchmark"@en ;
  iao:0000115 "Benchmark value for a WSP metric that helps delineate status zones (for example, the limit reference point for absolute abundance)."@en ; # definition
  iao:0000119 "Pestal, G. et al. (2023). State of the Salmon: Rapid Status Assessment Approach for Pacific Salmon under Canada’s Wild Salmon Policy."@en ; # definition source
  dcterms:source <https://publications.gc.ca/site/eng/9.928944/publication.html> ;
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:AlgorithmThreshold a owl:Class ;
  rdfs:label "Algorithm threshold"@en ;
  iao:0000115 "Threshold value used by the rapid-status algorithm (such as Learning Tree 3) that may differ from expert-defined benchmarks to incorporate precautionary buffers."@en ; # definition
  iao:0000119 "DFO (2024). Rapid Status Approximations for Pacific Salmon Derived from Integrated Expert Assessments under Canada’s Wild Salmon Policy. CSAS Science Response 2024/004."@en ; # definition source
  dcterms:source <https://www.dfo-mpo.gc.ca/csas-sccs/Publications/ScR-RS/2024/2024_004-eng.pdf> ;
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:ConfidenceCategory a owl:Class ;
  rdfs:label "Confidence category"@en ;
  iao:0000115 "Categorical confidence rating (High, Medium, Low) reported alongside a rapid status assessment."@en ; # definition
  rdfs:subClassOf iao:0000030 ; # Information content entity
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# FSAR controlled vocabularies
#################################################################

gcdfos:AbsoluteAbundanceMetric a skos:Concept ;
  rdfs:label "Absolute abundance metric"@en ;
  skos:prefLabel "Absolute abundance"@en ;
  iao:0000115 "Wild Salmon Policy metric comparing generational average spawner abundance to COSEWIC criterion D1 thresholds."@en ; # definition
  skos:inScheme :WSPMetricScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RelativeAbundanceMetric a gcdfos:WSPMetric , skos:Concept ;
  rdfs:label "Relative abundance metric"@en ;
  skos:prefLabel "Relative abundance"@en ;
  iao:0000115 "Wild Salmon Policy metric comparing generational average spawner abundance to CU-specific benchmarks (for example Sgen or 80% SMSY)."@en ; # definition
  skos:inScheme :WSPMetricScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:LongTermTrendMetric a gcdfos:WSPMetric , skos:Concept ;
  rdfs:label "Long-term trend metric"@en ;
  skos:prefLabel "Long-term trend"@en ;
  iao:0000115 "Wild Salmon Policy metric comparing current generational average to long-term average spawner abundance."@en ; # definition
  skos:inScheme :WSPMetricScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:PercentChangeMetric a gcdfos:WSPMetric , skos:Concept ;
  rdfs:label "Percent change metric"@en ;
  skos:prefLabel "Percent change"@en ;
  iao:0000115 "Wild Salmon Policy metric quantifying linear change in spawner abundance over the most recent three generations."@en ; # definition
  skos:inScheme :WSPMetricScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RapidStatusConfidenceHigh a gcdfos:ConfidenceCategory , skos:Concept ;
  rdfs:label "High rapid-status confidence"@en ;
  skos:prefLabel "High confidence"@en ;
  iao:0000115 "Rapid status confidence category indicating that high-quality abundance metrics support the result."@en ; # definition
  skos:inScheme :RapidStatusConfidenceScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RapidStatusConfidenceMedium a gcdfos:ConfidenceCategory , skos:Concept ;
  rdfs:label "Medium rapid-status confidence"@en ;
  skos:prefLabel "Medium confidence"@en ;
  iao:0000115 "Rapid status confidence category indicating mixed evidence or reliance on trend metrics supplemented by some abundance information."@en ; # definition
  skos:inScheme :RapidStatusConfidenceScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:RapidStatusConfidenceLow a gcdfos:ConfidenceCategory , skos:Concept ;
  rdfs:label "Low rapid-status confidence"@en ;
  skos:prefLabel "Low confidence"@en ;
  iao:0000115 "Rapid status confidence category indicating limited supporting data (for example, derived from trend metrics alone)."@en ; # definition
  skos:inScheme :RapidStatusConfidenceScheme ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

#################################################################
# FSAR object properties
#################################################################

gcdfos:hasStatusAssessment a owl:ObjectProperty ;
  rdfs:label "has status assessment"@en ;
  skos:prefLabel "has status assessment"@en ;
