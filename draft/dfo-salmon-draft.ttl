#################################################################

gcdfos:theme a owl:AnnotationProperty ;
  rdfs:label "theme"@en ;
  rdfs:comment "Associates OWL classes and SKOS concepts with high-level thematic concept schemes (e.g., Stock Assessment, GSI, Policy)." ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:publicationStatus a owl:AnnotationProperty ;
  rdfs:label "publication status"@en ;
  rdfs:comment "Indicates whether an ontology term is in draft status or ready for publication. Terms marked as PublishReady must have complete metadata (label, definition, definition source, isDefinedBy, and at least one theme)."@en ;
  rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> .

gcdfos:Draft a owl:NamedIndividual ;
  rdfs:label "Draft"@en ;
  iao:0000115 "Publication status indicating that a term is still under development and not yet ready for publication."@en ; # definition
