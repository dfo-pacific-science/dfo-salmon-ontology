#################################################################
# Roll-up / Partonomy pattern: MU ▶ CU ▶ Stock
# Use these between INDIVIDUALS (e.g., Skeena MU, its CUs, specific Stocks).
#################################################################

# Generic membership (transitive)
gcdfo:hasMember a owl:ObjectProperty , owl:TransitiveProperty ;
  rdfs:label "has member"@en ;
  skos:prefLabel "has member"@en ;
  iao:0000115 "Generic hierarchical containment/membership across management hierarchies (transitive)."@en ; # definition
  rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> .

gcdfo:isMemberOf a owl:ObjectProperty ;
  rdfs:label "is member of"@en ;
  skos:prefLabel "is member of"@en ;
  iao:0000115 "Inverse of hasMember."@en ; # definition
  owl:inverseOf gcdfo:hasMember ;
  rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> .

# Stock Management Unit ▶ Conservation Unit
gcdfo:hasMemberCU a owl:ObjectProperty ;
  rdfs:label "has member CU"@en ;
  skos:prefLabel "has member CU"@en ;
  iao:0000115 "A Stock Management Unit contains a Conservation Unit."@en ; # definition
  rdfs:domain gcdfo:StockManagementUnit ;
  rdfs:range  gcdfo:ConservationUnit ;
  rdfs:subPropertyOf gcdfo:hasMember ;
  rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> .

gcdfo:isMemberOfSMU a owl:ObjectProperty ;
  rdfs:label "is member of SMU"@en ;
  skos:prefLabel "is member of SMU"@en ;
  iao:0000115 "A Conservation Unit belongs to a Stock Management Unit."@en ; # definition
  rdfs:domain gcdfo:ConservationUnit ;
  rdfs:range  gcdfo:StockManagementUnit ;
  owl:inverseOf gcdfo:hasMemberCU ;
  rdfs:subPropertyOf gcdfo:isMemberOf ;
  rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> .

# Conservation Unit ▶ Stock (Population)
gcdfo:hasMemberStock a owl:ObjectProperty ;
  rdfs:label "has member stock"@en ;
  skos:prefLabel "has member stock"@en ;
  iao:0000115 "A Conservation Unit contains one or more Stocks (populations)."@en ; # definition
  rdfs:domain gcdfo:ConservationUnit ;
  rdfs:range  gcdfo:Stock ;
  rdfs:subPropertyOf gcdfo:hasMember ;
  rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> .

gcdfo:isMemberOfCU a owl:ObjectProperty ;
  rdfs:label "is member of CU"@en ;
  skos:prefLabel "is member of CU"@en ;
  iao:0000115 "A Stock (population) belongs to a Conservation Unit."@en ; # definition
  rdfs:domain gcdfo:Stock ;
  rdfs:range  gcdfo:ConservationUnit ;
  owl:inverseOf gcdfo:hasMemberStock ;
  rdfs:subPropertyOf gcdfo:isMemberOf ;
  rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> .

# Inference note:
# Because hasMemberCU ⊑ hasMember and hasMemberStock ⊑ hasMember, and hasMember is transitive:
# MU hasMemberCU CU  AND  CU hasMemberStock Stock  ⇒  MU hasMember Stock

gcdfo:hasReferencePoint a owl:ObjectProperty ;
