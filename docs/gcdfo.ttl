                          rdfs:range iop:Property ;
                          <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a salmon data variable to the measured/estimated property component in I-ADOPT decomposition."@en ;
                          rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                          rdfs:label "variable has property"@en ;
                          gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                      gcdfo:StockAssessmentTheme .


