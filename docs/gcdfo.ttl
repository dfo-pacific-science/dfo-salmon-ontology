                               rdfs:domain gcdfo:SalmonDataVariable ;
                               rdfs:range iop:Entity ;
                               <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a salmon data variable to an optional context object/entity component in I-ADOPT decomposition."@en ;
                               rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                               rdfs:label "variable has context object"@en ;
                               gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                           gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#variableHasMatrix
gcdfo:variableHasMatrix rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf iop:hasMatrix ;
                        rdfs:domain gcdfo:SalmonDataVariable ;
                        rdfs:range iop:Entity ;
                        <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a salmon data variable to an optional matrix/context entity component in I-ADOPT decomposition."@en ;
                        rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                        rdfs:label "variable has matrix"@en ;
                        gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                    gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#variableHasObjectOfInterest
gcdfo:variableHasObjectOfInterest rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf iop:hasObjectOfInterest ;
                                  rdfs:domain gcdfo:SalmonDataVariable ;
                                  rdfs:range iop:Entity ;
                                  <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a salmon data variable to the entity/object-of-interest component in I-ADOPT decomposition."@en ;
                                  rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                                  rdfs:label "variable has object of interest"@en ;
                                  gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                              gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#variableHasProperty
gcdfo:variableHasProperty rdf:type owl:ObjectProperty ;
