gcdfo:variableHasStatisticalModifier rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf iop:hasStatisticalModifier ;
                                     rdfs:domain gcdfo:SalmonDataVariable ;
                                     rdfs:range iop:StatisticalModifier ;
