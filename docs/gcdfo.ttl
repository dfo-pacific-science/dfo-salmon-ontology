                            rdfs:domain gcdfo:SalmonDataVariable ;
                            rdfs:range iop:Constraint ;
                            <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a salmon data variable to an optional constraint component in I-ADOPT decomposition."@en ;
                            rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                            rdfs:label "variable has constraint"@en ;
                            gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                        gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#variableHasContextObject
gcdfo:variableHasContextObject rdf:type owl:ObjectProperty ;
