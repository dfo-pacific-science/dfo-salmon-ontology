#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/ns/dqv#inDimension
dqv:inDimension rdf:type owl:ObjectProperty ;
                <http://purl.obolibrary.org/obo/IAO_0000115> "Represents the dimensions a quality metric, certificate and annotation allow a measurement of."@en ;
                <http://purl.obolibrary.org/obo/IAO_0000119> <https://www.w3.org/TR/vocab-dqv/> ;
                rdfs:comment "Dimensions are meant to systematically organize metrics, quality certificates and quality annotations. The Data Quality Vocabulary defines no specific cardinality constraints for dqv:inDimension, since distinct quality frameworks might have different perspectives over a metric. A metric may therefore be associated to more than one dimension. However, those who define new quality metrics should try to avoid this as much as possible and assign only one dimension to the metrics they define. More than one dimension can be indicated for each quality annotation or certificate."@en ;
                rdfs:isDefinedBy dqv: ;
                rdfs:label "in dimension"@en .


###  http://www.w3.org/ns/prov#generated
prov:generated rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasDerivedFrom
prov:wasDerivedFrom rdf:type owl:ObjectProperty .


###  https://w3id.org/gcdfo/salmon#conservationUnitOf
gcdfo:conservationUnitOf rdf:type owl:ObjectProperty ;
                         owl:inverseOf gcdfo:hasConservationUnit ;
                         rdfs:domain gcdfo:ConservationUnit ;
                         rdfs:range gcdfo:StockManagementUnit ;
                         <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a Conservation Unit to the Stock Management Unit it belongs to."@en ;
                         rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                         rdfs:label "Conservation Unit of"@en ;
                         gcdfo:theme gcdfo:FisheriesManagementTheme ,
                                     gcdfo:PolicyGovernanceTheme ,
                                     gcdfo:WildSalmonPolicyTheme .


###  https://w3id.org/gcdfo/salmon#demeOf
gcdfo:demeOf rdf:type owl:ObjectProperty ;
             owl:inverseOf gcdfo:hasDeme ;
             rdfs:domain gcdfo:Deme ;
             rdfs:range gcdfo:Population ;
             <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a deme to the population it belongs to."@en ;
             rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
             rdfs:label "deme of"@en ;
             gcdfo:theme gcdfo:GeneticsStockCompositionTheme ,
                         gcdfo:WildSalmonPolicyTheme .


###  https://w3id.org/gcdfo/salmon#hasConservationUnit
gcdfo:hasConservationUnit rdf:type owl:ObjectProperty ;
                          rdfs:domain gcdfo:StockManagementUnit ;
                          rdfs:range gcdfo:ConservationUnit ;
                          <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a Stock Management Unit to one of its constituent Conservation Units."@en ;
                          rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                          rdfs:label "has Conservation Unit"@en ;
                          gcdfo:theme gcdfo:FisheriesManagementTheme ,
                                      gcdfo:PolicyGovernanceTheme ,
                                      gcdfo:WildSalmonPolicyTheme .


###  https://w3id.org/gcdfo/salmon#hasDeme
gcdfo:hasDeme rdf:type owl:ObjectProperty ;
              rdfs:domain gcdfo:Population ;
              rdfs:range gcdfo:Deme ;
              <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a population to one of its constituent demes."@en ;
              rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
              rdfs:label "has deme"@en ;
              gcdfo:theme gcdfo:GeneticsStockCompositionTheme ,
                          gcdfo:WildSalmonPolicyTheme .


###  https://w3id.org/gcdfo/salmon#hasFeatureOfInterest
gcdfo:hasFeatureOfInterest rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf prov:used ;
                           rdfs:domain gcdfo:SurveyEvent ;
                           rdfs:range gcdfo:ReportingOrManagementStratum ;
                           <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a survey event to the reporting or management stratum that the event targets for inference or status interpretation."@en ;
                           rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                           rdfs:label "has feature of interest"@en ;
                           skos:closeMatch sosa:hasFeatureOfInterest ;
                           gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                       gcdfo:PolicyGovernanceTheme ,
                                       gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#hasObservationResult
gcdfo:hasObservationResult rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf prov:generated ;
                           rdfs:domain gcdfo:SurveyEvent ;
                           rdfs:range gcdfo:EscapementMeasurement ;
                           <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a survey event to a measurement datum/result it produces for salmon assessment workflows."@en ;
                           rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                           rdfs:label "has observation result"@en ;
                           skos:closeMatch sosa:hasResult ;
                           gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                       gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#hasObservedVariable
gcdfo:hasObservedVariable rdf:type owl:ObjectProperty ;
                          rdfs:domain gcdfo:SurveyEvent ;
                          rdfs:range gcdfo:SalmonDataVariable ;
                          <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a survey event to a salmon data variable specification that defines what was measured or derived."@en ;
                          rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                          rdfs:label "has observed variable"@en ;
                          skos:closeMatch sosa:observedProperty ;
                          gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                      gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#hasPopulation
gcdfo:hasPopulation rdf:type owl:ObjectProperty ;
                    owl:inverseOf gcdfo:populationOf ;
                    rdfs:domain gcdfo:ConservationUnit ;
                    rdfs:range gcdfo:Population ;
                    <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a Conservation Unit to one of its constituent populations."@en ;
                    rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                    rdfs:label "has population"@en ;
                    gcdfo:theme gcdfo:StockAssessmentTheme ,
                                gcdfo:WildSalmonPolicyTheme .


###  https://w3id.org/gcdfo/salmon#hasTargetOrLimitExploitationRate
gcdfo:hasTargetOrLimitExploitationRate rdf:type owl:ObjectProperty ;
                                       owl:inverseOf gcdfo:isTargetOrLimitForObservedExploitationRate ;
                                       rdfs:domain gcdfo:ObservedExploitationRate ;
                                       rdfs:range gcdfo:TargetOrLimitExploitationRate ;
                                       <http://purl.obolibrary.org/obo/IAO_0000115> "Relates an observed exploitation-rate estimate to the target, threshold, or limit exploitation-rate value used to evaluate it."@en ;
                                       rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                                       rdfs:label "has target or limit exploitation rate"@en ;
                                       gcdfo:theme gcdfo:FisheriesManagementTheme ,
                                                   gcdfo:PolicyGovernanceTheme ,
                                                   gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#hasYearBasis
gcdfo:hasYearBasis rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( gcdfo:EscapementMeasurement
                                               gcdfo:ExploitationRate
                                               gcdfo:ObservedExploitationRate
                                               gcdfo:SpawnerAbundance
                                               gcdfo:TargetOrLimitExploitationRate
                                               gcdfo:TotalExploitationRate
                                             )
                               ] ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( gcdfo:BroodYear
                                              gcdfo:CatchYear
                                              gcdfo:ReturnYear
                                            )
                              ] ;
                   <http://purl.obolibrary.org/obo/IAO_0000115> "Links a measurement or rate to the year basis (e.g., brood year, catch year, or return year) it is reported against."@en ;
                   rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                   rdfs:label "has year basis"@en ;
                   gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                               gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#isSampleOfStratum
gcdfo:isSampleOfStratum rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf prov:wasDerivedFrom ;
                        rdfs:domain dwc:MaterialEntity ;
                        rdfs:range gcdfo:ReportingOrManagementStratum ;
                        <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a material sample to the reporting or management stratum it is taken to represent in analysis."@en ;
                        rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                        rdfs:label "is sample of stratum"@en ;
                        skos:closeMatch sosa:isSampleOf ;
                        gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                    gcdfo:PolicyGovernanceTheme ,
                                    gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#isTargetOrLimitForObservedExploitationRate
gcdfo:isTargetOrLimitForObservedExploitationRate rdf:type owl:ObjectProperty ;
                                                 rdfs:domain gcdfo:TargetOrLimitExploitationRate ;
                                                 rdfs:range gcdfo:ObservedExploitationRate ;
                                                 <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a target, threshold, or limit exploitation-rate value to the observed exploitation-rate estimate it evaluates."@en ;
                                                 rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                                                 rdfs:label "is target or limit for observed exploitation rate"@en ;
                                                 gcdfo:theme gcdfo:FisheriesManagementTheme ,
                                                             gcdfo:PolicyGovernanceTheme ,
                                                             gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#populationOf
gcdfo:populationOf rdf:type owl:ObjectProperty ;
                   rdfs:domain gcdfo:Population ;
                   rdfs:range gcdfo:ConservationUnit ;
                   <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a population to the Conservation Unit it belongs to."@en ;
                   rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                   rdfs:label "population of"@en ;
                   gcdfo:theme gcdfo:StockAssessmentTheme ,
                               gcdfo:WildSalmonPolicyTheme .


###  https://w3id.org/gcdfo/salmon#usesObservationProcedure
gcdfo:usesObservationProcedure rdf:type owl:ObjectProperty ;
                               rdfs:domain gcdfo:SurveyEvent ;
                               rdfs:range <http://purl.obolibrary.org/obo/IAO_0000104> ;
                               <http://purl.obolibrary.org/obo/IAO_0000115> "Relates a survey event to the procedure/protocol used to execute it."@en ;
                               rdfs:isDefinedBy <https://w3id.org/gcdfo/salmon> ;
                               rdfs:label "uses observation procedure"@en ;
                               skos:closeMatch sosa:usedProcedure ;
                               gcdfo:theme gcdfo:DataModelProvenanceTheme ,
                                           gcdfo:StockAssessmentTheme .


###  https://w3id.org/gcdfo/salmon#variableHasConstraint
gcdfo:variableHasConstraint rdf:type owl:ObjectProperty ;
