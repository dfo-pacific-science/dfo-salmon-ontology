        <rdfs:comment xml:lang="en">Legacy methods: Helicopter, Fixed Wing Aircraft, Aerial, Drone, UAS, UAV</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
