        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">The percentile rank of a spawner abundance value (annual or period-specific) against a defined historical reference distribution.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2024. Rapid Status Approximations… CSAS Science Response 2024/004 (percentile benchmarks discussion).</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/41225260.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Spawner abundance percentile</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#SpawnerAbundanceTrend -->

