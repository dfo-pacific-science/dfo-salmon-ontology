        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">A trend metric computed from spawner abundance over time using a defined trend method (e.g., slope over a specified window).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Derived for SPSR term coverage; distinct from gcdfo:LongTermTrendMetric (current generational average vs long-term average) in CSAS Science Response 2024/004.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/41225260.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Spawner abundance trend</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#Stock -->

