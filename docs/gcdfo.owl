    <!-- https://w3id.org/gcdfo/salmon#hasYearBasis -->

    <owl:ObjectProperty rdf:about="https://w3id.org/gcdfo/salmon#hasYearBasis">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#EscapementMeasurement"/>
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#ExploitationRate"/>
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#ObservedExploitationRate"/>
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#SpawnerAbundance"/>
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#TargetOrLimitExploitationRate"/>
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#TotalExploitationRate"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#BroodYear"/>
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#CatchYear"/>
                    <rdf:Description rdf:about="https://w3id.org/gcdfo/salmon#ReturnYear"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <obo:IAO_0000115 xml:lang="en">Links a measurement or rate to the year basis (e.g., brood year, catch year, or return year) it is reported against.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">has year basis</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#DataModelProvenanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:ObjectProperty>
    


