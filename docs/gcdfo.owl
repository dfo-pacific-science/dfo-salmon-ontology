    <!-- https://w3id.org/gcdfo/salmon#Deme -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#Deme">
        <rdfs:subClassOf rdf:resource="http://rs.tdwg.org/dwc/terms/Organism"/>
        <obo:IAO_0000115 xml:lang="en">A local interbreeding group of organisms within a species. In salmon, a deme typically corresponds to a spawning population at a specific site. Demes are the finest biological unit and make up populations.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Deme</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#GeneticsStockCompositionTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#Escapement -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#Escapement">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000115 xml:lang="en">The number of mature salmon that pass through (or escape) fisheries and return to fresh water to spawn.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2005. Canada&apos;s Policy for Conservation of Wild Pacific Salmon. https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/315577.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Escapement</rdfs:label>
        <skos:closeMatch rdf:resource="http://purl.dataone.org/odo/SALMON_00000479"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#MonitoringFieldWorkTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#EscapementMeasurement -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#EscapementMeasurement">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <obo:IAO_0000115 xml:lang="en">A measurement or estimate of escapement tied to a stock and a survey event.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Escapement measurement</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#MonitoringFieldWorkTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#EscapementSurveyEvent -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#EscapementSurveyEvent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#SurveyEvent"/>
        <obo:IAO_0000115 xml:lang="en">A survey event specifically for estimating escapement.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Escapement Survey Event</rdfs:label>
        <skos:prefLabel xml:lang="en">Escapement Survey Event</skos:prefLabel>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#MonitoringFieldWorkTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ExploitationRate -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ExploitationRate">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <obo:IAO_0000115 xml:lang="en">A fishing mortality rate expressed as the proportion of the total return removed by fishing over a specified assessment period.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. Pacific Salmon - Glossary (Pacific Region). https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Exploitation rate</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#FisheriesManagementTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#FisheriesReferencePointLower -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#FisheriesReferencePointLower">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#LimitReferencePoint"/>
        <obo:IAO_0000115 xml:lang="en">A lower fisheries-management reference point defined for Stock Management Units (SMUs) and related fishery decision frameworks. It may be informed by Conservation Unit (CU) lower biological benchmarks, but is not identical to CU-level lower biological benchmarks used to delineate Wild Salmon Policy biological status zones.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2022). Methodologies and Guidelines for Defining Limit Reference Points for Pacific Salmon. DFO Can. Sci. Advis. Sec. Sci. Advis. Rep. 2022/030. (Erratum: December 2022).</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Fisheries Reference Point - Lower</rdfs:label>
        <skos:altLabel xml:lang="en">FRP-L</skos:altLabel>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#FisheriesManagementTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#PolicyGovernanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#GenerationalAverageAbundance -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#GenerationalAverageAbundance">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">The generational average of spawner abundance, calculated as a geometric mean across the number of years corresponding to the most common age at return (generation length), optionally after smoothing depending on CU group.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2024. Rapid Status Approximations for Pacific Salmon Derived from Integrated Expert Assessments under DFO’s Wild Salmon Policy. CSAS Pacific Region Science Response 2024/004.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/41225260.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Generational average abundance</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#DataModelProvenanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#InRiverMortalityRate -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#InRiverMortalityRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">The proportion of fish in a defined population/cohort that die during freshwater migration in-river on the way to spawning areas during a specified assessment interval, excluding landed catch (fishery removals) where those are accounted separately.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2025. Stock-Specific Fraser River Sockeye Salmon Run Size Determination. CSAS Pacific Region Science Advisory Report 2025/021 (en route mortality excluding landed catch). COSEWIC. 2017. Sockeye Salmon (Oncorhynchus nerka) assessment and status report (en-route loss is fish that perished in the river en-route to spawning grounds for reasons other than fishing).</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/41294622.pdf"/>
        <dcterms:source rdf:resource="https://www.canada.ca/en/environment-climate-change/services/species-risk-public-registry/cosewic-assessments-status-reports/sockeye-salmon-2017.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">In-river mortality rate</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#IndicatorRiver -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#IndicatorRiver">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ReportingOrManagementStratum"/>
        <obo:IAO_0000115 xml:lang="en">A reporting and assessment stratum identifying monitored indicator streams that provide representative indices of spawner abundance for nearby streams.</obo:IAO_0000115>
        <dcterms:source rdf:resource="https://www.salmonexplorer.ca/methods/glossary.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Indicator river</rdfs:label>
        <skos:altLabel xml:lang="en">Indicator stream</skos:altLabel>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#MonitoringFieldWorkTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#PolicyGovernanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#LimitReferencePoint -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#LimitReferencePoint">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ReferencePoint"/>
        <obo:IAO_0000115 xml:lang="en">A fish stock experiencing serious harm may be depleted to the point where it has impaired productivity or reproductive capacity. As a result, the stock would be less resilient to fishing and could not easily rebuild. Serious harm may be irreversible, or only slowly reversible over the long-term, and can be due to fishing, other human-induced impacts, or natural causes. Under the Precautionary Approach policy and the Fish Stocks Provisions (FSPs), breaching the LRP is a trigger for a rebuilding plan.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. Introduction to Stock Assessment - Glossary. https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://www.dfo-mpo.gc.ca/science/species-especes/fisheries-halieutiques/assessment-evaluation/glossary-glossaire/index-eng.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Limit reference point</rdfs:label>
        <skos:altLabel xml:lang="en">LRP</skos:altLabel>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#FisheriesManagementTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#PolicyGovernanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#LongTermTrendMetric -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#LongTermTrendMetric">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">A long-term trend metric comparing the current generational average (geometric mean) spawner abundance to the long-term average (geometric mean) spawner abundance for an assessed time series.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2024. Rapid Status Approximations for Pacific Salmon Derived from Integrated Expert Assessments under DFO’s Wild Salmon Policy. CSAS Pacific Region Science Response 2024/004.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/41225260.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Long-term trend metric</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#DataModelProvenanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#LowerBiologicalBenchmark -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#LowerBiologicalBenchmark">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#MetricBenchmark"/>
        <obo:IAO_0000115 xml:lang="en">Lower benchmark value for a Conservation Unit (CU) metric used to delineate Wild Salmon Policy status zones (typically the boundary between Red and Amber zones). This CU-level biological benchmark is distinct from SMU-level fisheries management lower reference points such as FRP-L.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. (2021). Wild Salmon Policy 2018-2022 Implementation Plan. Available at https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/40728109.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Lower biological benchmark</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#PolicyGovernanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#MainstemMortalityRate -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#MainstemMortalityRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">The proportion of fish that die while migrating through the freshwater mainstem portion of a river system (as operationally delimited by the assessment workflow), over a specified interval or between specified mainstem reference points.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">LGL Limited. Migration and Survival of Late-run Fraser Sockeye (radio-telemetry methods; fish detected at Mission are classified as survivors, fishery-related mortalities, or in-river mortalities; fixed-station tracking is conducted at strategic locations including mainstem confluences).</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/341440.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Mainstem mortality rate</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#MarineSurvivalIndex -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#MarineSurvivalIndex">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">A dimensionless index or covariate representing marine survival conditions or relative marine survival outcomes (not necessarily a survival proportion).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2021. Marine Survival Forecast of Southern British Columbia Coho (uses climate indicators correlated with marine survival; supports index-like predictors for survival conditions).</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/Library/40978680.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Marine survival index</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#HabitatEcosystemClimateTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#MetricBenchmark -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#MetricBenchmark">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000030"/>
        <obo:IAO_0000115 xml:lang="en">Benchmark value for a metric that helps delineate status zones (for example, the limit reference point for absolute abundance).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Pestal, G., MacDonald, B.L., Grant, S.C.H., and Holt, C.A. 2023. State of the salmon: rapid status assessment approach for Pacific salmon under Canada&apos;s Wild Salmon Policy. Canadian Technical Report of Fisheries and Aquatic Sciences 3570.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://publications.gc.ca/site/eng/9.928944/publication.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Metric benchmark</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ObservedExploitationRate -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ObservedExploitationRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ExploitationRate"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">An empirically estimated exploitation rate derived from observed or reconstructed catches/removals and returns/escapement for a specified stock and period.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. Pacific Salmon - Glossary (Pacific Region). https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Observed exploitation rate</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#DataModelProvenanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#FisheriesManagementTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000109"/>
        <obo:IAO_0000115 xml:lang="en">A measurement datum representing an empirically observed rate, count, or abundance derived from monitoring or survey data.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Observed rate or abundance</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#DataModelProvenanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#OceanMortalityRate -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#OceanMortalityRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">The proportion of fish that die during the ocean (marine) life-history phase for a defined population/cohort and interval; when a marine survival proportion is available, ocean mortality may be represented as its complement under the assessment’s accounting conventions.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">COSEWIC. 2017. Sockeye Salmon (Oncorhynchus nerka) assessment and status report (marine survival index described and interpreted in a Pacific salmon assessment context).</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://www.canada.ca/en/environment-climate-change/services/species-risk-public-registry/cosewic-assessments-status-reports/sockeye-salmon-2017.html"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Ocean mortality rate</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#PercentChangeMetric -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#PercentChangeMetric">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">A short-term trend metric quantifying the percent (linear) change in spawner abundance over the most recent three generations.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2024. Rapid Status Approximations for Pacific Salmon Derived from Integrated Expert Assessments under DFO’s Wild Salmon Policy. CSAS Pacific Region Science Response 2024/004.</obo:IAO_0000119>
        <dcterms:source rdf:resource="https://waves-vagues.dfo-mpo.gc.ca/library-bibliotheque/41225260.pdf"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Percent change metric</rdfs:label>
        <skos:altLabel xml:lang="en">Rapid-status percent change metric</skos:altLabel>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#DataModelProvenanceTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#Population -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#Population">
        <rdfs:subClassOf rdf:resource="http://rs.tdwg.org/dwc/terms/Organism"/>
        <obo:IAO_0000115 xml:lang="en">A group of organisms of the same species occupying a defined area that interbreed and share a gene pool. In salmon, populations are composed of one or more demes and form the biological basis for Conservation Units.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Population</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#GeneticsStockCompositionTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#StockAssessmentTheme"/>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#WildSalmonPolicyTheme"/>
    </owl:Class>
    


    <!-- https://w3id.org/gcdfo/salmon#ProbabilityDeclineBelowLowerBenchmarkMetric -->

    <owl:Class rdf:about="https://w3id.org/gcdfo/salmon#ProbabilityDeclineBelowLowerBenchmarkMetric">
        <rdfs:subClassOf rdf:resource="https://w3id.org/gcdfo/salmon#ObservedRateOrAbundance"/>
        <obo:IAO_0000115 xml:lang="en">A probability metric estimating the risk that spawner abundance will be below the lower benchmark, given uncertainty in benchmark estimates and/or abundance estimates, for a defined forecast or assessment horizon.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Fisheries and Oceans Canada. 2024. Rapid Status Approximations for Pacific Salmon Derived from Integrated Expert Assessments under DFO’s Wild Salmon Policy. CSAS Pacific Region Science Response 2024/004.</obo:IAO_0000119>
