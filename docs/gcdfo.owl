        <obo:IAO_0000115 xml:lang="en">Relates a Conservation Unit to the Stock Management Unit it belongs to.</obo:IAO_0000115>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/gcdfo/salmon"/>
        <rdfs:label xml:lang="en">Conservation Unit of</rdfs:label>
        <theme rdf:resource="https://w3id.org/gcdfo/salmon#FisheriesManagementTheme"/>
