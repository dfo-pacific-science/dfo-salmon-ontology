    skos:hasTopConcept gcdfos:TargetOrLimitRateOrAbundance ;
    skos:prefLabel "Biological Benchmark and Indicator Scheme"@en ;
    gcdfos:theme gcdfos:StockAssessmentTheme .

gcdfos:BroodYear a owl:Class ;
    rdfs:label "Brood year"@en ;
    iao:0000115 "The year that a cohort of salmon spawns."@en ;
    iao:0000119 "Government of Canada Pacific Salmon Glossary; verbatim in NCEAS Salmon Ontology."@en ;
    dcterms:source odo:SALMON_00000520,
        <https://www.pac.dfo-mpo.gc.ca/fm-gp/salmon-saumon/gloss-eng.html> ;
    rdfs:comment "The parental year for a group of returning salmon, i.e. the calendar year when the majority of parents of these fish spawned."@en ;
    rdfs:isDefinedBy <https://w3id.org/gcdfos/salmon> ;
    rdfs:subClassOf gcdfos:YearBasis ;
    skos:exactMatch odo:SALMON_00000520 ;
    gcdfos:theme gcdfos:StockAssessmentTheme .

